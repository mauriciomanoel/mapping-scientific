@mastersthesis{4676239,
                source={CatalogoTeses},
                author={KAROLYNE ANDRADE FERREIRA},
                title={Resiliência urbana e a gestão de riscos de escorregamentos: uma avaliação da defesa civil do município Santos - SP.},
                month={09},
                year={2016},
                school={UNIVERSIDADE DE SÃO PAULO - ENGENHARIA CIVIL},
                abstract={The concept of urban resilience has entered discussions of how cities can prepare, adapt and deal with natural disasters in a context of extreme events. resilience is understood as a process that involves learning and adaptation capabilities in order to reduce the risk of disasters, restore desired functions improve quality of life. Landslides constitute one of the disasters that caused more deaths in the municipality of Santos (the São Paulo state coast), however, number of fatalities dropped significantly after the implementation of the Civil Defense Preventive Plan (PPDC) operated annually since 1989. consideration that the municipal civil defense is the institution that deals directly with the issue of disasters, the aim of this research was to identify the concerning the landslide risk management used by the Civil Defense and analyze their application in promoting urban resilience. From qualitative including literature reviews, as well as document research and field research, the following instruments were identified: weather monitoring; a chart susceptibility to gravitational mass movements and floods, a risk reduction plan and a geotechnical aptitude to an urbanization chart. In conclusion, the of urban resilience by the Civil Defense of Santos is partially because the instruments are still in the implementation process and challenges need to be as the articulation of municipal departments in favor of risk reduction.},
                keywords={Civil defense - Santos (São Paulo (SP) Brazil);Environmental disasters;Landslides;Urban resilience},
                note={O conceito de resiliência urbana colabora nas discussões de como as cidades podem se preparar ou se adaptar para lidar com desastres naturais num de eventos extremos. Resiliência urbana entende-se enquanto processo que envolve capacidades de aprendizado e adaptação com vistas à redução desastres naturais, ao retorno às funções desejadas e à melhoria da qualidade de vida das pessoas. Os escorregamentos configuram um dos desastres mais causaram mortes no município de Santos, no litoral do Estado de São Paulo; no entanto, o número de vítimas fatais reduziu-se significativamente implementação do Plano Preventivo de Defesa Civil (PPDC) operado anualmente desde 1989. Entendendo que a Defesa Civil municipal é a instituição diretamente com a questão dos desastres, o objetivo desta pesquisa foi identificar os instrumentos relativos à gestão de risco de escorregamentos utilizados Defesa Civil de Santos e analisar a aplicação dos mesmos na promoção da resiliência urbana. A partir de uma pesquisa qualitativa que envolveu bibliográfica, pesquisa documental e pesquisa de campo, identificaram-se os seguintes instrumentos: Monitoramento Meteorológico; Carta de Suscetibilidade Movimentos Gravitacionais de Massa e Inundações, Plano Municipal de Redução de Riscos e Carta Geotécnica Morros de Santos e São Vicente. que a promoção da resiliência urbana pela Defesa Civil de Santos ocorre parcialmente, pois os instrumentos ainda estão em processo de implementação desafios precisam ser superados como a articulação das secretarias municipais em prol da redução de risco.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=4676239}
            }
@phdthesis{5838410,
                source={CatalogoTeses},
                author={DANIEL CARVALHO DOS SANTOS},
                title={PROPOSTA DE APLICATIVO SOBRE BALNEABILIDADE NAS PRAIAS DE MACEIÓ COM USO DAS CONCEPÇÕES DO DESIGN THINKING},
                month={04},
                year={2017},
                school={CENTRO DE ESTUDOS E SISTEMAS AVANÇADOS DO RECIFE (PE) - DESIGN},
                abstract={This research investigated a strategy for the proposal creation of an application whose goal is to share information in social media, by objective form about Maceió beaches, based on Design Thinking (DT) theory. The theoretical contribution of the study is based on Brown (2010), Pinheiro; Alt (2011) and Stickdorn; Schneider (2014). In the perspective of DT, to resolve a problem requires desirability, feasibility and practicality, so this proposal indicates potential for market execution, since to make an economically feasible idea, because is one of the criteria for the theory. The research also uses the Human Centered Design (HCD) proposal, which includes reflecting on people's desire to identify how they perceive the world and from there initiate studies for solutions that are both practical and feasible. This work had as main objective to create a proposal of application titled Aqqualys on water quality in the beaches of Maceió, from the perspective of the bathers. The methodology included the following steps: (i) Identification of the users at the most frequented beaches (Jatiúca, Pajuçara and Ponta Verde); (ii) interviews with swimmers; (iii) Creation of Personas; (iv) Analysis of results. In addition, the information was organized from the concept of smart cities and strategic design. The beaches in Maceió constitutes a relevant area of leisure for the population and tourists, and it is relevant to reflect on the public policies foreseen for this environment, including visibility on the monitoring of bathing, carried out in this city by the Instituto do Meio Ambiente (Ima). This study was motivated by the perception about the water quality of Maceió beaches and the use that the target public does of the same ones. For future studies it is planned to expand research in order to execute the proposal.},
                keywords={design thinking. water quality. Maceió. app.},
                note={Esta pesquisa utilizou uma estratégia para a criação da proposta de um aplicativo, cuja meta é compartilhar, em redes sociais, informações objetivas sobre as praias de Maceió, com base na abordagem Design Thinking (DT). O aporte teórico do estudo se fundamenta em Brown (2010), Pinheiro; Alt (2011) e Stickdorn; Schneider (2014). Na concepção do DT a resolução de um problema requer desejabilidade, viabilidade e praticabilidade, sendo assim a proposta indica potencial para execução mercadológica, já que tornar uma ideia economicamente viável é um dos critérios desta teoria. A pesquisa utiliza também a ideia Human Centered Design (HCD), que inclui refletir acerca do desejo das pessoas para identificar a forma como elas percebem o mundo e a partir daí iniciar estudos de soluções que sejam, simultaneamente, práticas e viáveis. Este trabalho teve como principal objetivo criar uma proposta de aplicativo intitulado Aqqualys sobre balneabilidade nas praias de Maceió, com base na perspectiva dos banhistas. A metodologia incluiu as seguintes etapas: (i) Identificação dos usuários das praias mais frequentadas (Jatiúca, Pajuçara e Ponta Verde); (ii) entrevistas com banhistas; (iii) Criação de Personas; (iv) Análise dos resultados. Além disso, as informações foram organizadas a partir do conceito de cidades inteligentes e do design estratégico. Verifica-se que as praias em Maceió, constituem espaço relevante de lazer para a população e turistas, sendo relevante a reflexão acerca das políticas públicas previstas para este ambiente entre as quais se inclui transparência sobre o monitoramento de balneabilidade, realizado na capital alagoana pelo Instituto do Meio Ambiente (Ima). Este estudo foi motivado pela percepção sobre a qualidade da água nas praias maceioenses e o uso que o público alvo faz das mesmas. Para estudos futuros é previsto ampliar pesquisas de modo a executar a proposta.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=5838410}
            }
@phdthesis{4222504,
                source={CatalogoTeses},
                author={IZAUTINO VANDERLEY PEREIRA DE OLIVEIRA},
                title={Play in Unusual Places},
                month={03},
                year={2016},
                school={CENTRO DE ESTUDOS E SISTEMAS AVANÇADOS DO RECIFE (PE) - DESIGN},
                abstract={The Playable City is a design experiment, a challenge that pushes designers, architects and artists to contextualize a more humane and functional world. Playable projects are seen as gateways to a more expressive city, and residents and tourists reconfigure and rewrite services, places and stories. By understanding the city operation through its residents point of view, it emerges into a worldview that refuses to accept data as the final realization of the space. The playable ideas have been explored in a variety of projects, ranging from street art to the streets of games. At this point, the flow of research is a playful approach that aims to contribute to the exchange of ideas and a better understanding of the use of public spaces. However, from a case study, this research examines how games can help to promote the engagement and participation of citizens in public areas. In addressing this issue, the survey creates debates about the prospects of the relationship between design, games, technology and public spaces.},
                keywords={Design. Fun. Games. Playable City. Playfull.},
                note={A Cidade Jogável é um experimento de design, um desafio que empurra designers, arquitetos e artistas a contextualizarem um mundo mais humano e funcional. Os projetos jogáveis são vistos como portas de entrada para uma cidade mais expressiva, além de permite aos moradores e visitantes reconfigurarem e reescreverem seus serviços, lugares e histórias. Entender o funcionamento da cidade a partir de seus residentes, emerge para uma visão de mundo que se recusa a aceitar dados como a realização final do espaço. As ideias playable têm sido exploradas em uma variedade de projetos, que vão da arte urbana até os jogos de ruas. Neste ponto, o fluxo de investigação é uma abordagem lúdica que visa contribuir para a troca de ideias e uma melhor compreensão da utilização dos espaços públicos. Entretanto, a partir de um estudo de caso, este trabalho de pesquisa analisa de que forma os jogos podem contribuir para promover o engajamento e a participação dos cidadãos em áreas públicas. Ao abordar esta questão, a pesquisa cria debates sobre as perspectivas da relação entre design, jogos, tecnologias e espaços públicos.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=4222504}
            }
@phdthesis{1096898,
                source={CatalogoTeses},
                author={DIEGO TAVARES DE SOUZA},
                title={SMART PATH: UMA ANÁLISE EXPLORATÓRIA DO TEMPO GASTO EM TRAJETOS URBANOS COM AUXÍLIO DE APLICATIVO MOBILE DE COLETA COLABORATIVA},
                month={04},
                year={2014},
                school={CENTRO DE ESTUDOS E SISTEMAS AVANÇADOS DO RECIFE - ENGENHARIA DE SOFTWARE},
                abstract={Traffic is one of the major factors that can cause problems and disorders in large cities nowadays. Issues related to traffic are directly affecting citizens every day, causing inconvenience and loss of time constantly. Nowadays, there are research and techniques studied to continually improve the service provided to the population, such as mobile applications that inform real-time data of the traffic situation. Based on these problems, this paper aims to analyze the data traffic of the city of Recife, capital of Pernambuco state, where we will create a mobile application as a support tool for collecting data daily of citizens traffic, with that, we will make an analysis exploratory those data in order to verify the impact of the departure time on the time spent in transit. In parallel, we will apply a questionnaire to know the opinion of the people about the traffic in the city, and then we will make a comparative analysis of the data in the application with the consolidated research. After analyzing the data, we found that the time outlet actually has an impact on the time path where the times between 8:00 am and 9:00 am and between 18:00 and 19:00 for the night shift, had the highest time spent in traffic, and we have also seen that the second and Friday are the days where users spend more time in transit.},
                keywords={Smart cities, Mobility.},
                note={O trânsito é um dos principais fatores que podem causar problemas e transtornos nas grandes metrópoles nos dias atuais. Todos os dias os cidadãos são impactados diretamente por problemas relacionados ao trânsito, causando transtornos e perda de tempo constante. Existem pesquisas e técnicas estudadas para melhorar continuamente este serviço prestado a população, como por exemplo, aplicações móveis que informam em tempo real dados sobre a situação do trânsito. Baseado nesses problemas, este trabalho tem como objetivo analisar dados do trânsito da cidade do Recife, capital do estado de Pernambuco, onde iremos criar uma aplicação móvel como ferramenta de apoio para coleta de dados diária no trânsito dos cidadãos, para com isso fazermos uma análise exploratória sobre esses dados, a fim de verificar o impacto do horário de saída no tempo gasto no trajeto. Em paralelo, aplicaremos um questionário para sabermos a opinião do povo sobre o trânsito na cidade, e em seguida, faremos um comparativo dos dados analisados na aplicação com os consolidados na pesquisa. Após analisar os dados, constatamos que o horário de saída realmente tem impacto no tempo do trajeto, onde os horários entre 8:00 e 9:00 da manhã e entre 18:00 e 19:00 para o turno da noite, apresentaram o maior tempo gasto no trânsito, e vimos também, que a segunda e a sexta-feira são os dias onde os usuários perdem mais tempo no trânsito.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=1096898}
            }
@phdthesis{1097690,
                source={CatalogoTeses},
                author={ANDRE GUSTAVO DIAS VASCONCELOS},
                title={Fermata Astuta - Uma proposta de acessibilidade para analfabetos em um sistema de parada de ônibus para ajudar na construção de cidades inteligentes.},
                month={04},
                year={2014},
                school={CENTRO DE ESTUDOS E SISTEMAS AVANÇADOS DO RECIFE - ENGENHARIA DE SOFTWARE},
                abstract={Fermata Astuta is a proposed tool that advertises the routes at the bus stop, allowing the identification of buses to people who cannot read. Improving urban mobility and enabling illiterate persons can play a more active role in society. The technical feasibility analysis was made evaluating the use of the tool together multiple routers and antennas. The need for a study which was found to define the set of routers and antennas will be installed. Well depending on where the equipment announces a few meters before or after. The validation was performed on a sample of illiterate and functionally illiterate community "Entra a pulso" in Recife, where 97% of subjects identified the bus route.},
                keywords={Mobility, Smart Cities.},
                note={Fermata Astuta é uma proposta de ferramenta que anuncia os itinerários na parada de ônibus, permitindo a identificação dos ônibus àquelas pessoas que não conseguem ler. Melhorando a mobilidade urbana e possibilitando que pessoas analfabetas possam desempenhar um papel mais ativo na sociedade. A análise de viabilidade técnica se deu avaliando o uso da ferramenta em conjunto de vários roteadores e antenas. Foi constatada a necessidade de um estudo para definir onde o conjunto de roteadores e antenas será instalado. Pois dependendo do local o equipamento anuncia alguns metros antes ou depois. A validação foi realizada com uma amostra de analfabetos e analfabetos funcionais da comunidade "Entra a pulso" em Recife onde 97% dos indivíduos identificaram o itinerário do ônibus.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=1097690}
            }
@phdthesis{1116646,
                source={CatalogoTeses},
                author={DANILO LEITE E SILVA},
                title={SUSCITE: SIMULATION SMART CITY TESTBED - UM AMBIENTE PARA VALIDAÇÃO DE APLICAÇÕES VOLTADAS PARA AMBIENTES URBANOS BASEADO EM SERVIÇOS},
                month={04},
                year={2014},
                school={CENTRO DE ESTUDOS E SISTEMAS AVANÇADOS DO RECIFE - ENGENHARIA DE SOFTWARE},
                abstract={Urban growth, along with changing scenarios of population distribution, where for the first time in history the majority of the population lives in large cities, such cities have motivated the constant development of solutions focused on support and meet this demand, which enters the concept of Smart Cities. In this context, it is possible to see the emergence of data based solutions focused on sensors scattered throughout the cities. In view of this, this research proposes a means of enable prior validation of solutions for cities building a tool to provide basic services of a city, as a TestBed mechanism, simply and without the need for extensive infrastructure, through services},
                keywords={Smart cities, Sensors, TestBed, Mobility.},
                note={O crescimento urbano, juntamente com a mudança de cenários de distribuição populacional, onde pela primeira vez na história a maior parte da população vive em grandes cidades, tem motivado tais cidades no constante desenvolvimento de soluções com foco em suportar e atender essa demanda, onde entra o conceito de Cidades Inteligentes. Nesse contexto, é possível perceber o surgimento de soluções baseadas nos dados dessas metrópoles que tem seu foco em sensores espalhados pelas cidades. Diante disto, esta pesquisa propõe uma forma de permitir a prévia validação das soluções para as cidades construindo uma ferramenta que disponibiliza serviços básicos de uma cidade, como um mecanismo de TestBed, de forma simples e sem necessidade de grande infra estrutura, por meio de serviços.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=1116646}
            }
@mastersthesis{2378443,
                source={CatalogoTeses},
                author={CRISTIANO RAMOS MOREIRA},
                title={UMA INICIATIVA DE SMART CITY: O ESTUDO DE CASO DO CENTRO INTEGRADO DE COMANDO DE PORTO ALEGRE},
                month={04},
                year={2015},
                school={PONTIFÍCIA UNIVERSIDADE CATÓLICA DO RIO GRANDE DO SUL - ADMINISTRAÇÃO E NEGÓCIOS},
                abstract={Given the population growth and increasing urbanization problems the need to search for creative solutions to improve the quality of life arises, which often are supported by the use of technology: thus the smart city initiatives come up. It is in this context that the present study aims to contribute by proposing to analyze a smart city initiative, to identify the challenges and success factors of its implementation, therefore contributing to the description of the phenomenon in the Brazilian scenario. The initiative chosen was the implementation of the Integrated Center of Command (CEIC) in Porto Alegre, because it is a service integration that uses technology. Hence, this study aims to answer the following research question: How is happening CEIC deployment in Porto Alegre? To answer this question, the Smart City Initiatives Framework in Chourabi et al. (2012) was used. To better understand the phenomenon under study, a qualitative approach with the case study method was used. Data were collected through semi-structured interviews in various organizational levels. Subsequently, to obtain the results the data were categorized. Thus, this research allowed to analyze the CEIC deployment and identified that the main contribution of the CEIC to the service integration was the implementation of a common, centralizedtechnology infrastructure through the development of a city monitoring infrastructure with the integration of cameras, images and information sharing with bodies that are part of CEIC and partners who work in special operations. In the study, the use of technology as a key element that helps to generate information to qualify the decision-making of municipal management, especially in emergencies and large events is highlighted. The key success factors identified were investment in technology, common technology infrastructure, training and collaboration. The main challenges evidenced were the scarcity of financial resources and the public procurement procedure.},
                keywords={E-gov. Smart Cities. Service Integration. CEIC},
                note={Diante do crescimento populacional e dos crescentes problemas de urbanização surge a necessidade de busca de soluções criativas para melhorar a qualidade de vida dos cidadãos, as quais, muitas vezes, são apoiadas pelo uso de tecnologia: assim surgem as iniciativas de smart city. É dentro deste contexto que o presente estudo pretende contribuir, buscando analisar uma iniciativa de smart city, identificar os desafios e fatores de sucesso de sua implantação, contribuindo, destarte, com a descrição do fenômeno no cenário brasileiro. A iniciativa escolhida foi a implantação do Centro Integrado de Comando (CEIC) da cidade de Porto Alegre, por tratar-se de uma integração de serviços através da utilização da tecnologia. Assim, este estudo busca responder à seguinte questão de pesquisa: como está ocorrendo a implantação do CEIC em Porto Alegre? Para respondê-la, foi utilizado o Framework Integrativo de Iniciativas de Smart Cities de Chourabi et al. (2012). Visando um melhor entendimento do fenômeno em estudo, buscou-se utilizar uma abordagem qualitativa, tendo como método o estudo de caso. O levantamento dos dados ocorreu através de entrevistas semiestruturadas em diversos níveis organizacionais. Posteriormente, para obtenção dos resultados, os dados foram categorizados. Assim, esta pesquisa permitiu analisar a implantação do CEIC e identificar que a principal contribuição do CEIC para a integração dos serviços foi implantar uma infraestrutura tecnológica comum e centralizada através do desenvolvimento de uma infraestrutura de monitoramento da cidade, com a integração de câmeras, compartilhando as imagens e informações com órgãos que integram o CEIC e parceiros que atuam em operações especiais. No estudo, destaca-se a utilização da tecnologia como elemento fundamental que auxilia a gerar informações para qualificar a tomada de decisão da gestão municipal, principalmente em situações emergenciais e eventos de grande porte. Os principais fatores de sucesso identificados foram investimento em tecnologia, infraestrutura tecnológica comum, treinamento e colaboração. Os principais desafios evidenciados foram a carência de recursos financeiros e o processo de aquisição pública.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=2378443}
            }
@phdthesis{6446987,
                source={CatalogoTeses},
                author={JOSIANE BRIETZKE PORTO},
                title={SMART CITIES METHODOLOGY (SCML): UMA METODOLOGIA EM SMART CITIES BASEADA EM VALOR PÚBLICO},
                month={08},
                year={2018},
                school={PONTIFÍCIA UNIVERSIDADE CATÓLICA DO RIO GRANDE DO SUL - ADMINISTRAÇÃO E NEGÓCIOS - PUC-RS - UCS},
                abstract={Smart city is an approach to managing and coping with urban challenges in search for innovative solutions leading to better quality of life and sustainability in cities. Several initiatives have been undertaken, with a significant worldwide growth trend in the coming years. Such initiatives, however, may require non-trivial public investment, and failures resulting from them can have important consequences such as monetary loss, loss of reputation, reduced confidence and lack of public value. This research aims at setting a methodology in smart cities composed of a reference model and an assessment method from the Public Value perspective. These artifacts were evaluated based on the perception of 23 representatives of the Quadruple Helix (government, industry, university and citizens) and on the results of the applicability in practice, through an initial pilot evaluation, in the city of Nova Santa Rita. It follows Design Science as its epistemological paradigm and Design Science Research as its method, uniting theoretical and methodological rigor as well as practical utility for society. The results showed that the artifacts developed in this research can help in the design and assessment of smart cities in a gradual way, bringing together best practices considered intelligent and that allow for the expansion and/or generation of Public Value, consisting of a prescriptive scientific contribution. Among the contributions are the protocol, contingency and construction heuristics, which detail how these artifacts were rigorously designed and developed, under the Design Science paradigm, to solve the problem identified in this research. They are specific and useful knowledge, generated from this research, aimed at the practice and resolution of real problem, and can be used for future evolutions of the artifacts developed in the research and/or design of new artifacts, in different contexts and classes of problems.},
                keywords={Smart Cities;Model Reference;Assessment Method;Public Value;Design Science Research.},
                note={Smart city corresponde a uma abordagem para gerenciamento e enfrentamento de desafios urbanos, em busca de soluções inovadoras para melhor qualidade de vida e sustentabilidade nas cidades. Em razão disso, diversas iniciativas vêm sendo feitas, com tendência de crescimento significativo nos próximos anos, em âmbito mundial. Entretanto, tais iniciativas podem requerer investimentos públicos não triviais e falhas podem ter consequências importantes como perda monetária, prejuízo em relação à reputação, redução de confiança e ausência de valor público para as partes interessadas. Este trabalho desenvolveu uma metodologia em smart cities, sob a perspectiva de Valor Público, composta por um modelo de referência e um método de avaliação, avaliados com base na percepção de 23 representantes da Quádrupla Hélice (governo, indústria, universidade e cidadãos) e nos resultados da aplicabilidade na prática, por meio de uma primeira avaliação piloto, na cidade gaúcha de Nova Santa Rita. Adota Design Science como paradigma epistemológico e Design Science Research como método de pesquisa, unindo rigor teórico-metodológico e utilidade prática para a sociedade. Os resultados mostraram que os artefatos desenvolvidos nessa pesquisa podem ajudar na concepção e na avaliação de smart cities de um modo gradual, reunindo melhores práticas consideradas inteligentes, que possibilitam ampliação e/ou geração de Valor Público, consistindo numa contribuição científica de caráter prescritivo. Entre as contribuições têm-se também o protocolo, as heurísticas contingenciais e de construção, que detalham como esses artefatos foram projetados e desenvolvidos com rigor, sob o paradigma de Design Science, para a resolução do problema identificado nessa pesquisa. Constituem conhecimento específico e útil, gerado a partir da pesquisa, voltado à prática e à resolução de um problema real, podendo ser usado para futuras evoluções dos artefatos desenvolvidos na pesquisa e/ou no projeto de novos artefatos, em diferentes contextos e classes de problemas.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=6446987}
            }
@mastersthesis{7451683,
                source={CatalogoTeses},
                author={NESTOR ALEJANDRO LUNA},
                title={Participação social na cidade inteligente: um estudo de caso na mobilidade urbana de Joinville},
                month={03},
                year={2018},
                school={FACULDADE MERIDIONAL - Administração},
                abstract={Smart City is a recent multidisciplinary phenomenon characterized by the use of technology, environmental sustainability and participatory governance with different models of social participation. A key component of this new urban proposal is the urban mobility which has direct influence on quality of life of cities with a high level of urbanization. This study aims to analyze how social participation happens in urban mobility policies in one of the fifty smartest cities in Brazil. An analytical matrix based on the Arnstein’s scale was developed and applied to the Joinville study case. The results shown that the proposals of social participation reach levels of pseudo-participation. Effective participation is affected by the presence of barriers, like inadequate schedules or arbitrary methods, and also the absence of facilitators as traceability or citizen empowerment. The tool developed to analyze social participation was considered well-fitted to identify the components and the level of commitment of governmental proposals with the social participation. It was also found that Smart City interventions still in an incipient stage. The smart city labeling responds to a protocol of intentions influenced by external agents and composed by projects disconnected from each other.  .},
                keywords={Smart Cities;Urban Mobility;Social Participation},
                note={A cidade inteligente é um fenômeno recente de abrangência multidisciplinar caracterizado pelo uso da tecnologia, a sustentabilidade ambiental e a governança participativa, instrumentalizada por diferentes modelos de participação social. A mobilidade urbana, componente chave dessa nova proposta urbana, se destaca por sua influência direta na qualidade de vida da população, sobretudo em cidades com alto índice de urbanização. Este estudo tem por objetivo analisar como acontece a participação social nas políticas de mobilidade urbana em uma das cinquenta cidades mais inteligentes do Brasil. Com esta finalidade uma matriz analítica baseada na escala de Arnstein foi desenvolvida e aplicada ao estudo de caso na cidade de Joinville. Os resultados mostram que os processos de participação social alcançam níveis de pseudo-participação; e que a participação efetiva está afetada pela presença de barreiras como agendas inadequadas e métodos impositivos, e pela ausência de facilitadores como rastreabilidade das opiniões e empoderamento do cidadão. Neste sentido, a matriz desenvolvida demonstrou-se adequada para identificar os componentes da participação social, bem como verificar o grau de compromisso das estratégias governamentais com a participação. Complementarmente, constatou-se que as intervenções da cidade “inteligente”, ainda se encontram em um estágio incipiente e que tal rotulação responde a um protocolo de intenções influenciado por agentes externos, composto por um portfólio de projetos desconectados entre si.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=7451683}
            }
@mastersthesis{3587746,
                source={CatalogoTeses},
                author={ALEXANDRE HEIDEKER},
                title={GERENCIAMENTO FLEXÍVEL DE INFRAESTRUTURA DE ACESSO PÚBLICO À INTERNET COM NFV},
                month={02},
                year={2016},
                school={UNIVERSIDADE FEDERAL DO ABC - ENGENHARIA DA INFORMAÇÃO},
                abstract={The concept of Smart Cities is already a reality with rapid urbanization and adoption of technologies like Internet of Things (IoT), smartphones and other pervasive computing technologies. This scenario presents a new class of computational and telecom infrastructure problems, requiring match flexibility, quality of service and cost, both financial and environmental. Technologies like Network Functions Virtualization (NFV), Cloud Computing and Software Defined Networks (SDN) are in evidence in recent years as a promise to address these demands. This paper uses the NFV approach to solving the problem of dynamic and elastic supply of public Internet access in major cities. The results show significant levels of performance and flexibility in addition to the choice of metrics and KVM, XEN and LXC container performance comparison in NFV.},
                keywords={NFV. Smart Cities. Network Virtualization.},
                note={O conceito de Cidades Inteligentes, com a rápida urbanização e adoção de tecnologias como Internet das Coisas (IoT), smartphones e outras tecnologias de computação pervasiva, já é realidade. Esse cenário apresenta uma nova classe de problemas computacionais e de infraestrutura de telecomunicações, exigindo o equacionamento das variáveis flexibilidade, qualidade de serviço e custo, tanto financeiro como ambiental. Tecnologias como Virtualização de Funções de Rede (NFV), Computação em Nuvem e Redes Definidas por Software (SDN) estão em evidência nos últimos anos como promessa para solucionar essas demandas. Este trabalho utiliza a abordagem NFV para solucionar o problema de fornecimento dinâmico e elástico de acesso público à internet nas grandes cidades. Os resultados obtidos demonstram níveis significativos de desempenho e flexibilidade, além da escolha de métricas e comparação de desempenho do KVM, XEN e containers LXC na virtualização de funções de rede.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=3587746}
            }
@phdthesis{5952613,
                source={CatalogoTeses},
                author={AERCIO CAVALCANTI DE MELO FILHO},
                title={IDENTITY MANAGEMENT AND SMART CITIES: A SYSTEMATIC MAPPING},
                month={01},
                year={2017},
                school={CENTRO DE ESTUDOS E SISTEMAS AVANÇADOS DO RECIFE (PE) - ENGENHARIA DE SOFTWARE},
                abstract={In 2008, according to the United Nation Population Fund (UNFPA), for the first time in our history, more than half of the world's population was living in urban centers. With this unprecedented population growth and increasing interest on the smart environments from industry and academia, there has been an increase in value-added services for societies. Within this context, the concept of Smart Cities emerged. The potential resultant from this increasing data exchange, however, raises serious privacy concerns for collection and processing of sensitive (personal) information will be recurrent for these events. Furthermore, in this new ecosystem, formed by a vast amount of assorted smart objects, the services provided are expected to be remotely accessed anytime, anywhere. This generates a sense of insecurity and privacy loss; thus, privacy and identity management solutions have to be prepared to handle the requirements imposed by these ecosystems. This work has the objective of identifying, evaluating, and interpreting published research that examine the current state of the art of Identity Management systems applied to interoperable environments.},
                keywords={Privacy. Security. Smart cities. Identity Management},
                note={Em 2008, segundo o Fundo de População das Nações Unidas (UNFPA), pela primeira vez em nossa história, mais da metade da população mundial vivia em centros urbanos. Com este crescimento populacional sem precedentes e o aumento no interesse da indústria e da academia em ambientes inteligentes, há um aumento nos serviços de valor agregado oferecido à sociedade. Nesse contexto, surgiu o conceito de Cidades Inteligentes. O potencial resultante é uma crescente troca de dados, que, no entanto, levanta sérias preocupações de privacidade durante a coleta e o processamento de informações sensíveis (pessoais). Além disso, nesse novo ecossistema, formado por uma vasta quantidade de objetos inteligentes variados, espera-se que os serviços fornecidos sejam acessados remotamente a qualquer hora e em qualquer lugar. Isso gera uma sensação de insegurança e perda de privacidade; Portanto, soluções de privacidade e gerenciamento de identidade precisam ser estruturadas para lidar com os requisitos impostos por esses ecossistemas. Este trabalho tem como objetivo identificar, avaliar e interpretar pesquisas publicadas que examinam o estado da arte dos sistemas atuais de Gerenciamento de Identidade aplicados a ambientes cidades inteligentes.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=5952613}
            }
@mastersthesis{7571595,
                source={CatalogoTeses},
                author={RENE DOUGLAS NOBRE DE MORAIS},
                title={Algoritmo inteligente para geração de rotas em smart cities},
                month={08},
                year={2018},
                school={UNIVERSIDADE FEDERAL RURAL DE PERNAMBUCO - Informática Aplicada},
                abstract={Congestion in large urban centers is one of the main problems for people who need to get around daily using public transport or their own. The emergence of the concept of smart cities has made possible the development of innumerable innovative solutions that, through technology, share and disseminate various information in real time to the population. However, as the problem is plagued by countless variables, it becomes increasingly difficult to deliver workable and timely solutions. In this way, this work proposes an approach in the tracing of sequential routes considering 3 objectives (travel time, distance and number of traffic lights) in large-scale road networks with timely congestion of vehicles using an evolutionary multiobjective algorithm. In addition, a new mutation operator that guarantees only minor changes in individuals is proposed. The proposed approach is tested by performing simulations on a map of a large city with different levels of congestion using Openstreetmap data.},
                keywords={smart cities;evolutionary algorithm;Openstreetmap},
                note={O congestionamento nos grandes centros urbanos é um dos principais problemas para as pessoas que necessitam se locomover diariamente seja utilizando o transporte público ou o próprio. O surgimento do conceito de cidades inteligentes tornou possível o desenvolvimento de inúmeras soluções inovadoras que, através da tecnologia, compartilham e disseminam diversas informações em tempo real para a população. Porém, a medida que o problema é tomado por inúmeras variáveis, torna-se cada vez mais difícil oferecer soluções viáveis e em tempo hábil. Desta forma, este trabalho propõe uma abordagem no traçado de rotas sequenciais considerando 3 objetivos (tempo de viagem, distância e número de semáforos) em redes viárias de larga-escala com congestionamento de veículos em tempo hábil utilizando um algoritmo evolucionário multiobjetivo. Além disso, um novo operador de mutação que garante apenas pequenas alterações nos indivíduos é proposto. A abordagem proposta é testada realizando simulações em um mapa de uma cidade grande com diferentes níveis de congestionamento utilizando dados do Openstreetmap.},
                url={https://sucupira.capes.gov.br/sucupira/public/consultas/coleta/trabalhoConclusao/viewTrabalhoConclusao.jsf?popup=true&id_trabalho=7571595}
            }
