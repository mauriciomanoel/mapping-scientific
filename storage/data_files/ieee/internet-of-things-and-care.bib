@INPROCEEDINGS{8494843, 
author={Z. Kazemi and A. Papadimitriou and D. Hely and M. Fazcli and V. Beroulle},   booktitle={2018 IEEE 3rd International Verification and Security Workshop (IVSW)}, 
 title={Hardware Security Evaluation Platform for MCU-Based Connected Devices: Application to Healthcare IoT}, 
  year={2018},   volume={},   number={},   pages={87-92},   abstract={Hardware security-based threats are often ignored or poorly considered by embedded system developers who focus on their system's functional specifications. Most embedded systems utilize software-based security mechanisms such as right management or cryptography, nevertheless they can be easily defeated by hardware attacks if no specific care is given during the development of the software. Consequently, it is necessary to provide software developers with efficient tools to assist in designing robust embedded systems against hardware security threats. In this work, we focus on medical embedded systems including a microcontroller unit (MCU) running the main application. We firstly show that the software running on the MCU is vulnerable against hardware attacks; then, we present an evaluation platform dedicated to MCU-based design to evaluate and secure software early in the design phase. We finally present preliminary results obtained with this platform, considering the side channel resilience of modern MCUs.},   keywords={Hardware;Software;Cryptography;Tools;Medical services;Safety;Hardware security;Internet of Things (IoT);embedded security;Hardware attacks;power supply glitch attack},   doi={10.1109/IVSW.2018.8494843},   ISSN={},   month={July},}
@INPROCEEDINGS{7064034, 
author={D. Jiang and L. Li and Z. Fan and J. Liu},   booktitle={2014 International Conference on Identification, Information and Knowledge in the Internet of Things}, 
 title={Detection of Acute Hypotensive Episodes via Empirical Mode Decomposition and Genetic Programming}, 
  year={2014},   volume={},   number={},   pages={225-228},   abstract={Big data time series in the Intensive Care Unit (ICU) is now touted as a solution to help clinicians to diagnose the case of the physiological disorder and select proper treatment based on this diagnosis. Acute Hypotensive Episodes (AHE) is one of the hemodynamic instabilities with high mortality rate that is frequent among many groups of patients. This study presented a methodology to predict AHE for ICU patients based on big data time series. Empirical Mode Decomposition (EMD) was used to calculate patient's Mean Arterial Pressure (MAP) time series and some features, which are bandwidth of the amplitude modulation, frequency modulation and power of Intrinsic Mode Function (IMF) were extracted. Then, the Genetic Programming (GP) is used to build the classification model for detection of AHE. The methodology was applied in the datasets of the 10th Physio Net and Computers Cardiology Challenge in 2009 and Multi-parameter Intelligent Monitoring for Intensive Care (MIMIC-II). We achieve the accuracy of 83.33% in the training set and 91.89% in the testing set of the 2009 challenge's dataset, and the 83.37% in the training set and 80.64% in the testing set of the MIMIC-II dataset.},   keywords={Big Data;diseases;feature extraction;genetic algorithms;medical signal processing;pattern classification;time series;acute hypotensive episodes detection;empirical mode decomposition;genetic programming;hemodynamic instabilities;mortality rate;intensive care unit;ICU patients;big data time series;mean arterial pressure;patients MAP time series;amplitude modulation bandwidth;frequency modulation;IMF power;intrinsic mode function;GP;classification model;AHE detection;10th PhysioNet;Computers Cardiology Challenge;Multiparameter Intelligent Monitoring for Intensive Care;MIMIC-II dataset;feature extraction;Time series analysis;Feature extraction;Training;Testing;Frequency modulation;Genetic programming;Bandwidth;acute hypotensive episodes;empirical mode decomposition;genetic programming;classification},   doi={10.1109/IIKI.2014.53},   ISSN={},   month={Oct},}
@INPROCEEDINGS{8260988, 
author={J. Markendahl and S. Lundberg and O. Kordas and S. Movin},   booktitle={2017 Internet of Things Business Models, Users, and Networks}, 
