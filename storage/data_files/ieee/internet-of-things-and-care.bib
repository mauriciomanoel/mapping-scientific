  year={2016},   volume={},   number={},   pages={1-3},   abstract={Internet of things (IoT) is about billions of heterogeneous smart devices connected through heterogeneous communications technologies such as Ethernet, WiFi, Bluetooth, ZigBee, etc. The variety of devices and technologies pushes the development of solutions and applications in various domains. Nevertheless, it makes the management, control and programmability of networks and IoT applications (Smart city, Smart healthcare, Industry, etc.) extremely complex. Therefore, we propose an SDN-based architecture to manage heterogeneity of the devices and the networks. Our solution is based on the utilization of dockers implemented on devices. In this paper, we focus on the connectivity of heterogeneous devices beyond heterogeneous networks. Our test results prove the feasibility of our architecture with the use of a centralized SDN controller and MiniNet emulator.},   keywords={Internet of Things;software defined networking;telecommunication network management;SDN-based architecture;IoT heterogeneity;Internet of Things;heterogeneous smart devices;heterogeneous communication technologies;dockers;heterogeneous networks;centralized SDN controller;MiniNet emulator;Internet of Things (IoT);Software Defined Networking (SDN);Docker;Heterogeneity},   doi={10.1109/SCNS.2016.7870558},   ISSN={},   month={Dec},}
@INPROCEEDINGS{8212747, 
