  year={2014},   volume={},   number={},   pages={49-54},   abstract={A new push-based mechanism of information exchange between medical devices using the MQ Telemetry Transport (MQTT) protocol for the Internet of Things (IoT) is presented. The proposed implementation allows any medical device to become interoperable without knowledge of the existence of other devices, and provides a simple, scalable solution to the current lack of medical device connectivity. Our approach facilitates the use of shared information in ways that were not envisioned by the original device manufacturer, thus enabling the continued independent improvement of patient safety and information integration throughout the life cycle of devices. We have implemented a VitalBridge interface running on Wifi-enabled Beaglebone Black embedded devices. This allows existing legacy devices to connect to this new communication framework, and use it to implement an integrated mobile monitoring and team communication platform for the intensive care unit.},   keywords={biomedical telemetry;open systems;patient care;transport protocols;wireless LAN;push-based mechanism;information exchange;MQ telemetry transport protocol;MQTT protocol;Internet of Things;IoT;medical device connectivity;shared information;patient safety;information integration;VitalBridge interface;Wifi-enabled Beaglebone Black embedded devices;legacy devices;integrated mobile monitoring;team communication platform;intensive care unit;Monitoring;Biomedical monitoring;Protocols;Internet of things;Hospitals;Medical diagnostic imaging;medical devices;interoperability;mq telemetry transport protocol;machine-to-machine communication;publish-subscribe information exchange},   doi={10.1109/IOT.2014.7030114},   ISSN={},   month={Oct},}
