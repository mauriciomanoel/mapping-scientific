  year={2018},   volume={},   number={},   pages={130-133},   abstract={In our previous work, a runtime-specific rule was introduced that described how to detect situations and invoke actions of connected vehicles in the surrounding context. However, rule developers were required to check carefully whether each rule was consistent to the other rules or not. Our next challenge is how to simplify the validation of the application logics when the number of rules is increasing. In this paper, we propose IoT Context Descriptor, a context model for situation detection and action invocation for IoT that is aiming at accelerating easy development of situation detection and action invocation while securing the processing of real-time high-volume transactions.},   keywords={Internet of Things;ubiquitous computing;real-time high-volume transactions;runtime-specific rule;connected vehicles;surrounding context;rule developers;context model;situation detection;action invocation;easy development;IoT Context Descriptor;Roads;Real-time systems;Context modeling;Vehicle dynamics;Internet of Things;XML;Data models;Internet of Things (IoT);model-driven development;multi-agent system;connected vehicles},   doi={10.1109/ICIOT.2018.00025},   ISSN={},   month={July},}
