@INPROCEEDINGS{8422823, 
author={M. Li and F. R. Yu and P. Si and H. Yao and Y. Zhang},   booktitle={2018 IEEE International Conference on Communications (ICC)}, 
 title={Software-Defined Vehicular Networks with Caching and Computing for Delay-Tolerant Data Traffic}, 
  year={2018},   volume={},   number={},   pages={1-6},   abstract={With the explosion in the number of connected devices and Internet of Things (IoT) services in smart city, the challenges to meet the demands from both data traffic delivery and information processing are increasingly prominent. Meanwhile, the connected vehicle networks have become an essential part in smart city, bringing massive data traffic as well as significant networking, caching and computing resources. In this paper, we propose a novel vehicle network architecture, mitigating the network congestion with the joint optimization of networking, caching and computing. Cloud computing at the data centers as well as mobile edge computing (MEC) at the evolved node Bs (eNodeBs) and on-board units (OBUs) are taken as the paradigms to provide caching and computing resources. The programmable control principle originated from software-defined networking (SDN) paradigm has been introduced to facilitate the system architecture and resource integration. With the careful modeling of the services, the vehicle mobility and the system state, a joint resource management scheme is proposed and formulated as a partially observable Markov decision process (POMDP) to minimize system cost, which consists of both network overhead and execution time of computing tasks. Extensive simulation results with different system parameters reveal that the proposed scheme could significantly improve the system performance compared to the existing schemes.},   keywords={cloud computing;Internet of Things;Markov processes;resource allocation;software defined networking;telecommunication traffic;vehicular ad hoc networks;smart city;network congestion;cloud computing;data centers;mobile edge computing;software-defined networking paradigm;software-defined vehicular networks;delay-tolerant data traffic;information processing;vehicle networks;Markov decision process;Internet of Things;vehicle network architecture;Servers;Cloud computin