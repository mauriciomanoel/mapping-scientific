@INPROCEEDINGS{7876242, 
author={M. Etemad and M. Aazam and M. St-Hilaire},   booktitle={2017 International Conference on Computing, Networking and Communications (ICNC)}, 
 title={Using DEVS for modeling and simulating a Fog Computing environment}, 
  year={2017},   volume={},   number={},   pages={849-854},   abstract={With the increase in popularity of Internet of Things (IoT), pervasive computing, healthcare services, sensor networks, and mobile devices, a lot of data is being generated at the perception layer. Cloud is the most viable solution for data storage, processing, and management. Cloud also helps in the creation of further services, refined according to the context and requirement. However, being reachable through the Internet, cloud is not efficient enough for latency sensitive multimedia services and other time-sensitive services, like emergency and healthcare. Fog, an extended cloud lying within the proximity of underlying nodes, can mitigate the issues traditional cloud cannot solve being standalone. Fog can provide quick response to the requiring applications. Moreover, it can preprocess and filter data according to the requirements. Trimmed data is then sent to the cloud for further analysis and enhanced service provisioning. However, how much better is it to have a fog in any particular scenario instead of a standalone cloud working without fog is a question right now. In this paper, we provide an answer by analyzing both cloud-only and cloud-fog scenarios in the context of processing delay and power consumption according to increasing number of users, on the basis of varying server load. The simulation is done through Discrete Event System Specification (DEVS). Simulation results demonstrate that by the use of fog networks, users experienced lower waiting times and increased data rates.},   keywords={cloud computing;data handling;discrete event systems;file servers;multimedia computing;power aware computing;Web services;DEVS;fog computing environment modeling;fog computing environment simulation;Internet of Things;IoT;pervasive computing;healthcare services;sensor networks;mobile devices;perception layer;data storage;data processing;data management;Internet;latency sensitive multimedia services;time-sensitive services;extended cloud computing;enhanced service provisioning;cloud-only scenarios;cloud-fog scenarios;processing delay;power consumption;discrete event system specification;fog networks;Fog computing;cloud computing;performance evaluation;Simulation;DEVS},   doi={10.1109/ICCNC.2017.7876242},   ISSN={},   month={Jan},}
@INPROCEEDINGS{8073633, 
author={K. Jayavel and V. Nagarajan and K. Sornalakshmi and K. Navin},   booktitle={2017 International Conference on IoT and Application (ICIOT)}, 
 title={Migration techniques of data centric networks to internet of things with control plane based virtual sensor layer}, 
  year={2017},   volume={},   number={},   pages={1-8},   abstract={Data centric networks are application specific and proprietary as against address centric networks which are uniquely addressable and standardized. Internet of Things which is a culmination of data centric and address centric networks have brought in a need to integrate various technologies which were otherwise in silos. The basic requirement of Internet of Things (IoT), is connecting anything, anytime and anywhere. Thus, our paper attempts to provide a brief idea of existing methodologies towards integration and propose a new architecture for Internet of Things towards integration of these most useful networks to the internet. As the volume of things connected have increased multifold have brought in new challenges with respect to monitoring and management of resources and data. Thus, this architecture is inspired from Software Defined concepts and attempts to divide the monitoring and the actual data into two layers namely the control and the data plane. The control plane will take care of dynamic resource discovery and provisioning based on user needs. The data plane deals only with pure/raw data. This model reduces the complexity by abstracting the control task from the data bounded physical layer.},   keywords={computer network management;Internet of Things;software defined networking;virtualisation;wireless sensor networks;virtual sensor layer;data centric networks;address centric networks;data plane;control plane based virtual sensor layer;Internet of Things;software defined concepts;dynamic resource discovery;data bounded physical layer;Protocols;Internet of Things;Wireless sensor networks;Standards;Servers;Service-oriented architecture;Data centric networks;Internet of Things (IoT);Software Defined Network(SDN)},   doi={10.1109/ICIOTA.2017.8073633},   ISSN={},   month={May},}
