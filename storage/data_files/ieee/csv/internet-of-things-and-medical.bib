  year={2016},   volume={},   number={},   pages={29-33},   abstract={With the analysis on hospital medical devices management operation, find the medical devices management style of many hospitals are still on the manual side, in lending, returning, warehousing and it bring many problems such as heavy workload, low efficiency, lack of intelligence. Combined with the technology of radio frequency identification (RFID) can rapid, high-volume, non-contact read the tag features and low price, easier to achieve system management characteristics, worked out an intelligent medical devices management system based on RFID technology. So as to realize the capabilities of quickly circulation, real time inventory, batch management of devices and so on. It greatly reduce the difficulty of managing medical devices, improve the efficiency of medical devices to find and to reduce the operation cost of the hospital, and increase the turnover rate of the devices.},   keywords={biomedical equipment;hospitals;radiofrequency identification;intelligent medical devices management system;RFID technology;hospital medical devices management operation;radiofrequency identification;real time inventory;batch device management;Radiofrequency identification;Biomedical equipment;Hospitals;Metals;Instruments;Software;Personnel;RFID technology;Internet of things;Medical devices management;System application},   doi={10.1109/ISAI.2016.0016},   ISSN={},   month={June},}
