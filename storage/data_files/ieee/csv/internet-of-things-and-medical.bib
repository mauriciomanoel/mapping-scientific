 title={Real-time performance management of assisted living services for Bluetooth low energy sensor communication}, 
  year={2017},   volume={},   number={},   pages={1143-1148},   abstract={PerfMon is a prototype implementation of a real-time performance management method for sensor data communication in assisted living applications. It is implemented in accordance with the specification for GATT services in Bluetooth low energy (BLE). PerfMon provides a tool for real-time performance monitoring and control for caregivers and service providers. Test results from monitoring and control of packet loss ratio related to alarm thresholds are presented. PerfMon is adapted to cloud-based web services using RESTful APIs and established object models. Performance management is a necessary component in an overall management system of IoT devices for healthcare and assisted living applications.},   keywords={assisted living;Bluetooth;data communication;health care;Internet of Things;medical computing;real-time performance management;assisted living services;Bluetooth low energy sensor communication;PerfMon;cloud-based Web services;RESTful API;sensor data communication;GATT services;BLE;IoT devices;healthcare;Trade agreements;Monitoring;Protocols;Bluetooth;Servers;Performance evaluation;Logic gates;performance monitoring;performance management;Internet of Things;Bluetooth low energy;Generic Attribute (GATT) service;ambient assisted living},   doi={10.23919/INM.2017.7987452},   ISSN={},   month={May},}
@INPROCEEDINGS{8284475, 
author={M. Banday},   booktitle={2017 International Conference on Computing and Communication Technologies for Smart Nation (IC3TSN)}, 
 title={Enhancing the security of IOT in forensics}, 
  year={2017},   volume={},   number={},   pages={193-198},   abstract={The implementation of forensics in IOT has made investigations much easy then the traditional investigation. The mission in investigating an IoT crime is delivered by the dynamic nature of IOT solutions. Many predominant generation areas, which incorporates cloud computing, cellular gadgets, computer systems, sensors and RFID (Radio Frequency identification) technology are the combination of IOT. Forensic science is a mix of two Latin phrases: forensics and science. Forensic, relating to examination accomplished in public as trials in the ancient international were commonly held in public, it also consists of a robust judicial connotation. The second one termed as science, which is nowadays closely tied to the medical technique, also a systematic manner of obtaining knowledge. Collectively, forensic technology may be seen as the usage of the medical approaches and resources in crime solving. No matter its historical etymology, forensic science is something concrete but old fashioned. Branches of forensic science are rooted in nearly each department of technological knowledge and many different elements of current society. Due to its capability to discover nowadays it is identified as a critical part of the judicial world. At the same time as the key evidence in crook cases may additionally have come from witnesses or other subjective manner simply meaning forensic evidence, based as it is at the scientific method, is seen as more decisive than even eyewitness' testimony. Especially in a judicial system like ours the accused is considered harmless till tested guilty, evidence gathered by forensic scientists is regularly utilized by both the protection and the prosecution in many of the judicial cases. The internet of things (IOT) accompanies both the central security and protection issues. Forensics for digital world although has been considered in every scholarly community and industry, portability crime scene investigation is phenomenally new and unexplored. Crime scene investigation manages rigging and methods that work towards forensically stable recuperation of data and confirmation from our cell phones or mobile devices.},   keywords={criminal law;digital forensics;Internet of Things;IoT crime;RFID technology;forensic science;medical approaches;judicial system;Internet of Things;central security issue;protection issue;Internet of Things;Sensors;Security;Cellular phones;Digital forensics;Smart grids;IOT;RFID;Forensics;Mobility Forensics},   doi={10.1109/IC3TSN.2017.8284475},   ISSN={},   month={Oct},}
