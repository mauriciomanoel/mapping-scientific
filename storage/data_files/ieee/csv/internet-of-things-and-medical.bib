 title={VITESSE - more intelligence with emerging technologies for health systems}, 
  year={2016},   volume={},   number={},   pages={1-3},   abstract={VITESSE is a low cost system to support users in two scenarios: home care and accidents (fainting, trampling, etc.). Initially, the system was based on the digital TV technology in scenarios of home care. Nowadays, the system adds new functions to support urgent and emergency care of individuals in mobility. In both cases, the key idea of VITESSE is to improve the time of consuming process, taking into account the real time and contextual information, in particular in the case of accidents of mobile users. Therefore, VITESSE is a context-aware system that makes use of the concept of Internet of Things (IoT) and ontologies in the process of generating inferences, increasing the efficiency of health care systems.},   keywords={health care;Internet of Things;medical computing;VITESSE;health systems;home care;accidents;fainting;trampling;digital TV technology;contextual information;mobile users;context-aware system;Internet-of-Things;IoT;ontologies;health care systems;Accidents;Digital TV;Ontologies;Sensors;Senior citizens;Medical services;Monitoring},   doi={10.1109/NOF.2016.7810131},   ISSN={},   month={Nov},}
@INPROCEEDINGS{7454584, 
author={A. Manashty and J. Light and U. Yadav},   booktitle={2015 17th International Conference on E-health Networking, Application   Services (HealthCom)}, 
 title={Healthcare event aggregation lab (HEAL), a knowledge sharing platform for anomaly detection and prediction}, 
  year={2015},   volume={},   number={},   pages={648-652},   abstract={Due to the increase in elderly population, research in healthcare monitoring and ambient assisted living technology is crucial to provide improved care and at the same time contain the healthcare cost. Among existing systems, there is none robust system that can act as a bridge between different systems to facilitate knowledge sharing, so as to empower the detection and prediction capabilities of them. These systems cannot use the data and knowledge of other similar systems due to the complexity involved in sharing data between them. Storing the information is also a challenge due to a high volume of sensor data generated by each sensor. The proposed HEAL model is a platform that provides services to developers to leverage the previously processed similar data and the corresponding detected symptoms. The proposed architecture is cloud-based and provides services for input sensors, Internet of Things devices, and context providers. The ultimate goal of the system is to fill the gap between symptoms and diagnosis trend data in order to predict health anomalies accurately and quickly.},   keywords={geriatrics;health care;patient diagnosis;patient monitoring;healthcare event aggregation lab;knowledge sharing platform;elderly population;healthcare monitoring;ambient assisted living technology;robust system;data generation;HEAL model;detected symptoms;cloud-based services;diagnosis trend data;Real-time systems;Hidden Markov models;Medical services;Context;Sensors;Data models;Predictive models;Sensor data;Healthcare knowledge sharing;Event Aggregation;Context-aware;Predict anomalies},   doi={10.1109/HealthCom.2015.7454584},   ISSN={},   month={Oct},}


@INPROCEEDINGS{8257668, 
author={M. Suryanegara and A. S. Arifin and M. Asvial and G. Wibisono},   booktitle={2017 4th International Conference on Information Technology, Computer, and Electrical Engineering (ICITACEE)}, 
 title={A system engineering approach to the implementation of the Internet of Things (IoT) in a country}, 
  year={2017},   volume={},   number={},   pages={20-23},   abstract={This paper proposes a framework for the system engineering approach to the Internet of Things (IoT) implementation in a country. The approach utilizes a system of hierarchical levels providing the perspectives of the “world view”, “domain view” and “element view” of IoT service applications. The world view of IoT is further refined into the domain views of the three actors which each have a specific domain of interest in a country-related dimension. These are the domains of the Policy Maker, the Industry, and the Market. Each of the domain views are then elaborated into several element views. The main contribution of this study is that the proposed framework can be used for guidance by policy makers - in particular, in a developing country which has just begun to consider a massive implementation of IoT services. As a new form of technology, IoT has some opportunities, as well as its challenges. This framework may help policy makers to identify crucial issues that need to be addressed.},   keywords={Internet of Things;systems engineering;system engineering approach;IoT service applications;country-related dimension;IoT services;Internet of Things;Security;Smart cities;Systems engineering and theory;Privacy;Medical services;IoT;Internet of Things;System Engineering;Policy},   doi={10.1109/ICITACEE.2017.8257668},   ISSN={},   month={Oct},}
@INPROCEEDINGS{6682351, 
author={Z. Jiyang and X. Shengwei and L. Nansen and W. Mixia and C. Xinxia},   booktitle={2013 IEEE International Conference on Green Computing and Communications and IEEE Internet of Things and IEEE Cyber, Physical and Social Computing}, 
 title={Spike Detection Based on Fractal Dimension}, 
  year={2013},   volume={},   number={},   pages={1834-1838},   abstract={Spikes detection is very important to the neural information study. However, the original neural signals collected by the microelectrode contain a lot of noise. Sometimes, spikes detection is hard to achieve when SNR (Signal to Noise Rate) is very low. At present, the fractal theory has been widely applied, and fractal dimension is very sensitive to fluctuation of curves. The fractal theory is introduced to the preprocessing of neural signals in this paper. It detected spikes by calculating fractal dimension of the original data. Experiments show that, fractal dimension can sign fluctuation of curve. This method can effectively detect the low amplitude spikes in the noise. The effect of spike detection based on fractal dimension is better than the usual threshold method and energy method.},   keywords={biomedical electrodes;fractals;medical signal detection;neurophysiology;spikes detection;neural information;microelectrode;SNR;signal to noise rate;fractal theory;fractal dimension;curves fluctuation;neural signals preprocessing;amplitude spikes;threshold method;energy method;Fractals;Equations;Mathematical model;Signal to noise ratio;Fluctuations;Algorithm design and analysis;spike;fractal dimension;signal-to-noise ratio},   doi={10.1109/GreenCom-iThings-CPSCom.2013.340},   ISSN={},   month={Aug},}
