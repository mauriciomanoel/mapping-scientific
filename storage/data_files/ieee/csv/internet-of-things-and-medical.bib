@INPROCEEDINGS{8276855, 
author={Y. Ma and G. Dagnino and I. Georgilas and S. Dogramadzi},   booktitle={2017 IEEE International Conference on Internet of Things (iThings) and IEEE Green Computing and Communications (GreenCom) and IEEE Cyber, Physical and Social Computing (CPSCom) and IEEE Smart Data (SmartData)}, 
