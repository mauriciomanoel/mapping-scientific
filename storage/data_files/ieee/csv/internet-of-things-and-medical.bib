  year={2016},   volume={},   number={},   pages={1-5},   abstract={The aim of our study is the design and development of IOT enabled smart, portable, non-invasive, assistive and precise medical tool based on the methods of Electrical Impedance Tomography which can be deployed for early diagnosis and location of cancerous tissues. EIT is a futuristic medical imaging technique through which one can clinically analyze the image of the body part acquired through surface voltage measurements. Our design incorporates 16 ECG electrodes for current injection and estimating the surface potential using the common ground current injection method, along with analog instrumentation and a microcomputer for processing. The design also features the advance capabilities of wireless data transmission and cloud based processing for analyzing the results.},   keywords={biological tissues;biomedical imaging;cancer;computerised tomography;electric impedance imaging;electrocardiography;Internet of Things;microcomputers;voltage measurement;smart assistive electrical impedance tomographic tool;clinical imaging;IOT enabled smart medical tool;portable medical tool;noninvasive medical tool;assistive medical tool;precise medical tool;cancerous tissue diagnosis;cancerous tissue location;medical imaging;surface voltage measurements;ECG electrodes;surface potential;common ground current injection;analog instrumentation;microcomputer;wireless data transmission;cloud based processing;Tomography;Electrodes;Phantoms;Impedance;Electric potential;Image reconstruction;Conductivity;Electrical Impedance Tomography;Wireless;Multi-frequency;Voltage Controlled Oscillator;Howland Current Pump;Filters;Raspberry Pi;Practical Phantom;Image Reconstruction;MATLAB;EIDORS},   doi={10.1109/WECON.2016.7993463},   ISSN={},   month={Oct},}
@INPROCEEDINGS{7552069, 
author={H. Fotouhi and A. Causevic and K. Lundqvist and M. Björkman},   booktitle={2016 IEEE 40th Annual Computer Software and Applications Conference (COMPSAC)}, 
 title={Communication and Security in Health Monitoring Systems -- A Review}, 
  year={2016},   volume={1},   number={},   pages={545-554},   abstract={The fast development of sensing devices and radios enables more powerful and flexible remote health monitoring systems. Considering the future vision of the Internet of Things (IoT), many requirements and challenges rise to the design and implementation of such systems. Bridging the gap between sensor nodes on the human body and the Internet becomes a challenging task in terms of reliable communications. Additionally, the systems will not only have to provide functionality, but also be highly secure. In this paper, we provide a survey on existing communication protocols and security issues related to pervasive health monitoring, describing their limitations, challenges, and possible solutions. We propose a generic protocol stack design as a first step toward handling interoperability in heterogeneous low-power wireless body area networks.},   keywords={body area networks;Internet of Things;medical computing;patient monitoring;protocols;security of data;telemedicine;remote health monitoring systems;Internet of Things;IoT;communication protocols;security issues;pervasive health monitoring;protocol stack design;heterogeneous low-power wireless body area networks;Monitoring;Security;Wireless communication;IEEE 802.15 Standard;Peer-to-peer computing;IEEE 802.11 Standard},   doi={10.1109/COMPSAC.2016.8},   ISSN={0730-3157},   month={June},}
@INPROCEEDINGS{7857601, 
