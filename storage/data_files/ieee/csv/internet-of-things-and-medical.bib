@INPROCEEDINGS{8077872, 
author={S. Shekhar and S. Sankar and A. K. Jain},   booktitle={2017 IEEE International Symposium on Electromagnetic Compatibility   Signal/Power Integrity (EMCSI)}, 
 title={Electrical modeling for sub-millivolt noise in compact IoT system designs}, 
  year={2017},   volume={},   number={},   pages={231-236},   abstract={All IoT systems require multiple sensors to collect environment information via analog to digital convertor (ADC). In several medical applications and industrial instrumentation, signals need to be captured with high resolution therefore the ADC requires an extremely quiet reference. This paper presents modeling and physical design considerations for a 12-bit ADC reference voltage supply to meet a 244 μV noise target. A fully differential approach is used in the electromagnetic and circuit modeling to capture noise on all parts of the passive network. Noise quantification accuracy is studied for different analyses that are part of the design flow: FEA, macro-modeling &amp; SPICE simulation. Noise reduction in physical design through PVT invariant band gap and electromagnetic isolation through Faraday cage are investigated.},   keywords={analogue-digital conversion;biomedical electronics;biomedical equipment;finite element analysis;medical signal processing;passive networks;signal denoising;signal resolution;SPICE;electrical modeling;sub-millivolt noise;compact IoT system designs;multiple sensors;environment information;digital convertor;medical applications;industrial instrumentation;extremely quiet reference;physical design considerations;12-bit ADC reference voltage supply;fully differential approach;electromagnetic circuit modeling;noise quantification accuracy;design flow;noise reduction;noise target;macro-modeling;PVT invariant band gap;electromagnetic isolation;Faraday cage;voltage 244.0 muV;Integrated circuit modeling;SPICE;Scattering parameters;Photonic band gap;Sensors;Sensitivity;Finite element analysis;Low noise;sensor;power integrity;IoT;reference voltage;FEM Solver;SPICE;macro modeling;ADC;ADC resolution},   doi={10.1109/ISEMC.2017.8077872},   ISSN={2158-1118},   month={Aug},}
