  year={2016},   volume={},   number={},   pages={481-486},   abstract={As to studies on the human brain, researchers merely have focused on the structure, function, etc. of the encephalic region, and the cascading failure of the encephalic region is yet to be explored. Here, we recruited 15 age-matched normal people as the experimental group. We got Pearson correlation coefficient of 90 brain regions by using the resting brain functional connectivity and structured connection matrix by using one sample T test. Then, according to the load capacity model algorithm, each brain area node was assigned a certain amount of load. We selected the important node of the default mode network (DMN) to attack, and found that the scale of big degree attack cascading failure was greater than the small degree attack. Protecting the important brain areas is of great significance to prevent the brain diseases.},   keywords={biomedical MRI;brain;matrix algebra;medical image processing;statistical analysis;human brain;default mode network;DMN;cascading failure algorithm;Pearson correlation coefficient;brain functional connectivity;connection matrix;load capacity model algorithm;functional magnetic resonance imaging;fMRI;Power system faults;Power system protection;Load modeling;Magnetic resonance imaging;Correlation;Brain modeling;default network;cascading failure;single sample T test;functional magnetic resonance imaging},   doi={10.1109/SNPD.2016.7515944},   ISSN={},   month={May},}
@INPROCEEDINGS{7793281, 
