  year={1999},   volume={},   number={},   pages={121-124},   abstract={In this paper we derive algorithms to identify the mixing matrix in the context of an independent component analysis with more sources than sensors. First, by exploiting the fact that for complex-valued observations, depending on the type of complex symmetry, 2 different fourth-order cumulants are available, we develop a technique that can cope with N(N+1)/2 sources for only N sensors. Secondly, the technique presented in Cardoso et al. (1994), based on a single cumulant, is modified to take both cumulants into account as well.},   keywords={higher order statistics;signal processing;matrix algebra;symmetry;identification;ICA techniques;blind source separation;sensors;mixing matrix identification;independent component analysis;complex-valued observations;complex symmetry;fourth-order cumulants;Independent component analysis;Tensile stress;Matrix decomposition;Helium;Symmetric matrices;Eigenvalues and eigenfunctions;Costs},   doi={10.1109/HOST.1999.778707},   ISSN={},   month={June},}


@INPROCEEDINGS{7380444, 
author={A. Singh and A. Thakur and A. Taparia},   booktitle={2015 International Conference on Green Computing and Internet of Things (ICGCIoT)}, 
 title={Analysis of computer vision and sensor technologies to assist the visually impaired}, 
  year={2015},   volume={},   number={},   pages={135-138},   abstract={Assistive technologies in the medical industry have been on an ever increasing trend in the past few years. The past few years have seen some great medical innovations which are helping cure and prevent diseases which were earlier thought incurable. Despite such huge improvements, many of the engineering innovations are yet to be realized to its full potential in the medical industry. The field of assistive technologies for the visually impaired has been such a realm. Assistive technologies have been there for many years but the scope and application of these technologies have been very limited. The recent advances in the computer vision and inertial navigation systems can be easily employed in this field to give much more control to the visually impaired. The advent of low cost IMU's and GPS has led to huge advancements in these areas. This paper discusses how these assistive technologies can be employed to benefit some of the less privileged section of the society.},   keywords={computer vision;handicapped aids;computer vision;sensor technologies;assistive technologies;medical industry;medical innovations;engineering innovations;inertial navigation systems;IMU;GPS;Navigation;Magnetic resonance imaging;Magnetometers;Biomedical imaging;Industries;Acoustics;Cameras;Assistive technologies;GPS;IMU;Image Processing},   doi={10.1109/ICGCIoT.2015.7380444},   ISSN={},   month={Oct},}
@INPROCEEDINGS{7827639, 
author={Teng Xu and M. Potkonjak},   booktitle={2016 IEEE/ACM International Conference on Computer-Aided Design (ICCAD)}, 
 title={Energy-efficient fault tolerance approach for Internet of Things applications}, 
  year={2016},   volume={},   number={},   pages={1-8},   abstract={Fault tolerance (FT) is essential in many Internet of Things (IoT) applications, in particular in the domains such as medical devices and automotive systems where a single fault in the system can lead to serious consequences. Non-volatile memory (NVM), on the other hand, is commonly used to improve system reliability due to its unique properties to retain data even if the power supply is lost. However, one of the most important drawbacks of NVM is that it imposes significant overhead regarding timing and energy. In this paper, we have proposed a unique technique with the use of NVM to create FT application specific architecture with almost no timing overhead and low energy overhead. We address the implementation of applications that are specified using synchronous data flow model of computation. We combine the use of NVM and classical CMOS transistors so that NVM judiciously stores selected complete states of the pertinent program. It allows the program to resume from the saved state in NVM when faults occur. The frequency of the state selection can be flexibly adjusted for an arbitrarily specified FT timing/energy overhead. Moreover, to find an optimal state selection (with low overhead), we have applied an improved min-cut max-flow algorithm. On a variety of typical benchmarks, the simulation results indicate that our approach incurs only a small overhead over lower bounds. It is also generic in a sense that it can be applied to a wide spectrum of underlying IoT architectures and computational models.},   keywords={CMOS memory circuits;energy conservation;fault tolerance;Internet of Things;minimax techniques;random-access storage;IoT architectures;mincut maxflow algorithm;CMOS transistors;synchronous data flow model;FT;NVM;nonvolatile memory;Internet of Things;energy-efficient fault tolerance},   doi={10.1145/2966986.2967034},   ISSN={1558-2434},   month={Nov},}
