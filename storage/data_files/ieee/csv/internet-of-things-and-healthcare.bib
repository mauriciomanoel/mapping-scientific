  year={2017},   volume={},   number={},   pages={607-612},   abstract={Research on activity based computing has, recently made progress and is growing attention in many real life applications like healthcare monitoring using wearable sensors and development of rehabilitation systems. However there is a lack of overview about the various sensing technologies used for activity recognition. In this paper, we present a comprehensive survey about the various sensing technologies used for activity recognition and the description of the various phases in activity recognition. The paper first discusses about the process of activity recognition using sensors, it further introduces various sensing technologies used for the process of activity recognition. Then, we review various methodologies used for the pre-processing phase, feature extraction phase and classification phase along with the application domain of the various techniques.},   keywords={feature extraction;inertial systems;intelligent sensors;smart phones;activity recognition process;inertial sensors;activity based computing;wearable sensors;sensing technologies;smartphone sensors;rehabilitation systems;preprocessing phase;feature extraction phase;classification phase;Activity recognition;Accelerometers;Hidden Markov models;Feature extraction;Support vector machines;Activity Recognition;Inertial Sensors;Smartphone Sensors;Internet of Things(IoT);Bioinformatics},   doi={10.1109/CCAA.2017.8229873},   ISSN={},   month={May},}
@INPROCEEDINGS{7993888, 
