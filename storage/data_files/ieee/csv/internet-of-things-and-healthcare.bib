@INPROCEEDINGS{6803139, 
author={A. Mashhadi and F. Kawsar and U. G. Acer},   booktitle={2014 IEEE World Forum on Internet of Things (WF-IoT)}, 
 title={Human Data Interaction in IoT: The ownership aspect}, 
  year={2014},   volume={},   number={},   pages={159-162},   abstract={As Internet of Things (IoT) becomes a growing reality, more ubiquitous devices are embedded in our daily lives, serving us in a broad range of purposes in everyday life from: personal healthcare to home automation to tailored smart city services. These devices primarily collect data that is about or produced by people, be it street noise level of a neighbourhood, or the energy footprint of an individual's home or her location and other situational context. As this unprecedented amount of data is collected, we are challenged with one fundamental research question: who owns this data and who should have access to it? Specifically, the emergent of the Human Data Interaction (HDI) topic which aims to put the human at the centre of the data driven industry, calls attention to the IoT community to address the data ownership aspect more carefully. In this note, we offer a reflection on the challenges that IoT faces in regards to the data ownership in HDI and advocate the roles that both ordinary people and industries must play to best answer those challenges in shaping the IoT landscape.},   keywords={human computer interaction;Internet of Things;ubiquitous computing;human data interaction;ownership aspect;Internet of Things;ubiquitous devices;personal healthcare;tailored smart city services;energy footprint;HDI;data driven industry;IoT community;data ownership;Data models;Companies;Biological system modeling;Internet;Home appliances;Home automation;Access control},   doi={10.1109/WF-IoT.2014.6803139},   ISSN={},   month={March},}
