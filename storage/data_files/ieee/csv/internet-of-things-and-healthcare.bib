@ARTICLE{8370750, 
author={P. Pace and G. Aloi and R. Gravina and G. Caliciuri and G. Fortino and A. Liotta},   journal={IEEE Transactions on Industrial Informatics}, 
 title={An Edge-based Architecture to Support Efficient Applications for Healthcare Industry 4.0}, 
  year={2018},   volume={},   number={},   pages={1-1},   abstract={Edge computing paradigm has attracted many interests in the last few years as a valid alternative to the standard cloud-based approaches to reduce the interaction timing and the huge amount of data coming from IoT devices toward the Internet. In the next future, edge-based approaches will be essential to support time-dependent applications in the Industry 4.0 context; thus the paper proposes &lt;formula&gt;&lt;tex&gt;${BodyEdge}$&lt;/tex&gt;&lt;/formula&gt;, a novel architecture well suited for human-centric applications in the context of the emerging healthcare industry. It consists of a tiny mobile client module and a performing gateway supporting multi-radio and multi-technology communication to collect and locally process data coming from different scenarios; moreover, it also exploits the facilities made available from both private and public cloud platforms to guarantee a high flexibility, robustness and adaptive level of service. The advantages of the designed software platform have been evaluated in terms of reduced bandwidth and processing time through a real implementation on different hardware platforms. The conducted study also highlighted the network conditions (data load and processing delay) in which &lt;formula&gt;&lt;tex&gt;${BodyEdge}$&lt;/tex&gt;&lt;/formula&gt; is a valid and inexpensive solution for healthcare application scenarios.},   keywords={Cloud computing;Medical services;Edge computing;Computer architecture;Logic gates;Industries;Delays;Body Sensor Networks;Edge Computing;Cloud Computing;Internet of Things;Heart Rate Variability},   doi={10.1109/TII.2018.2843169},   ISSN={1551-3203},   month={},}
@INPROCEEDINGS{682721, 
author={P. Pochec},   booktitle={Conference Proceedings. IEEE Canadian Conference on Electrical and Computer Engineering (Cat. No.98TH8341)}, 
 title={Modelling the performance of computer mirroring with difference queues}, 
  year={1998},   volume={1},   number={},   pages={217-220 vol.1},   abstract={The paper describes modelling of the performance of a special case of two computer systems operating in parallel: computer mirroring. In computer mirroring one of the systems is identified as the main and the other as the backup computer; both computers execute the same tasks. Each system operates independently and consists of an execution unit and a queue of tasks. Task scheduling and execution are modelled as Poisson processes. The performance of the mirroring system is modelled as a difference queue. The conditions for and the steady state solution for the performance of the computer mirroring system are derived. The processing rate /spl mu//sub 2/ of the backup system in computer mirroring should be no less than the product of the processing rate /spl mu//sub 1/ of the main system and the utilization rate /spl rho//sub 1/ of the main system, i.e. /spl rho//sub 1//spl mu//sub 1/&lt;/spl mu//sub 2/.},   keywords={performance evaluation;processor scheduling;parallel processing;queueing theory;stochastic processes;back-up procedures;processing rate;difference queues;performance modelling;parallel operation;backup computer;execution unit;task scheduling;task execution;Poisson processes;difference queue;steady state solution;computer mirroring system;product;utilization rate;Concurrent computing;Processor scheduling;Costs;Delay estimation;Computer science;Steady-state;Mission critical systems;Application software;Loss measurement;Protection},   doi={10.1109/CCECE.1998.682721},   ISSN={0840-7789},   month={May},}


