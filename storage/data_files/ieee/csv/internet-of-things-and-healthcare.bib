@INPROCEEDINGS{7870426, 
author={N. Kwak and S. Kim and K. H. Lee and C. Baek and M. S. Jang and Y. Joo and S. Lee and W. Y. Lee and E. Lee and D. Han and J. Kang and J. H. Lim and J. Park and K. Kim and S. Cho and S. W. Han and J. Y. Keh and J. H. Chun and J. Oh and S. H. Lee},   booktitle={2017 IEEE International Solid-State Circuits Conference (ISSCC)}, 
