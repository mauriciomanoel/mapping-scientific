@article{LESSARD20071754,
title = "Complexity and reflexivity: Two important issues for economic evaluation in health care",
journal = "Social Science & Medicine",
volume = "64",
number = "8",
pages = "1754 - 1765",
year = "2007",
issn = "0277-9536",
