}
@article{DAUTENHAHN2002397,
title = "From embodied to socially embedded agents – Implications for interaction-aware robots",
journal = "Cognitive Systems Research",
volume = "3",
number = "3",
pages = "397 - 428",
year = "2002",
note = "Situated and Embodied Cognition",
issn = "1389-0417",
doi = "https://doi.org/10.1016/S1389-0417(02)00050-5",
