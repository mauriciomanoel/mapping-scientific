@article{FAWCETT1994253,
title = "Responses to Vaginal Birth After Cesarean Section",
journal = "Journal of Obstetric, Gynecologic & Neonatal Nursing",
volume = "23",
number = "3",
pages = "253 - 259",
year = "1994",
issn = "0884-2175",
