title="International Journal of Industrial Ergonomics",
doc="S0169814117300641",
link_pdf="https://www.sciencedirect.com/science/article/pii/S0169814117300641/pdfft?md5=8ea231541cabb5061a6024fde067bc0c&pid=1-s2.0-S0169814117300641-main.pdf"
}
