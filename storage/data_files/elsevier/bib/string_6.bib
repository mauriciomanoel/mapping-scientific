@article{MATHWICK201011,
title = "Computer-Mediated Customization Tendency (CMCT) and the Adaptive e-Service Experience",
journal = "Journal of Retailing",
volume = "86",
number = "1",
pages = "11 - 21",
year = "2010",
issn = "0022-4359",
doi = "https://doi.org/10.1016/j.jretai.2009.11.001",
