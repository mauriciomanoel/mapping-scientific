title="Women and Birth",
doc="S1871519219309199",
link_pdf="https://www.sciencedirect.com/science/article/pii/S1871519219309199/pdfft?md5=19391a09a32132f544c8be922027862a&pid=1-s2.0-S1871519219309199-main.pdf"
}
