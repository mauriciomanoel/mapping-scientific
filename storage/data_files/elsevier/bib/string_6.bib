title="Procedia Computer Science",
doc="S1877050913000045",
link_pdf="https://www.sciencedirect.com/science/article/pii/S1877050913000045/pdfft?md5=a8fb1c8e00764b47db75a255899051cc&pid=1-s2.0-S1877050913000045-main.pdf"
}
