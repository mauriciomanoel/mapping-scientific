}
@article{HAM1988591,
title = "Computer-Aided Process Planning: The Present and the Future",
journal = "CIRP Annals",
volume = "37",
number = "2",
pages = "591 - 601",
year = "1988",
issn = "0007-8506",
doi = "https://doi.org/10.1016/S0007-8506(07)60756-2",
