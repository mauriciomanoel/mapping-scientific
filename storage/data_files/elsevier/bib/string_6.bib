}
@article{MARQUES201911,
title = "Coastal systems in transition: The game of possibilities for sustainability under global climate change",
journal = "Ecological Indicators",
volume = "100",
pages = "11 - 19",
year = "2019",
note = "Sven Erik Jørgensen - Memorial Issue",
issn = "1470-160X",
doi = "https://doi.org/10.1016/j.ecolind.2017.10.055",
url = "http://www.sciencedirect.com/science/article/pii/S1470160X17306921",
author = "João Carlos Marques",
keywords = "Sustainability challenges, Environmental management scenarios, Conceptual problems, Ecological sustainability trigon (EST), Estuaries and coastal zones",
abstract = "To achieve ecological sustainability and sustainable development represents a crucial challenge for the human kind, which depends on numerous driving forces, frequently opposing each other. Nevertheless, when dealing for instance with estuaries and coastal areas, three major drivers may in any case be identified, which are the search for human well-being in its several aspects, the conservation of environmental equilibrium, essential to ecological sustainability, and systems’ resilience in face of an increasing human pressure, resulting from population size and prosperity demand. To build and test environmental management scenarios involves therefore complex conceptual problems, demanding appropriate conceptual tools as for example the Ecological Sustainability Trigon (EST). Conceptually, the EST allows addressing the referred main drivers by using the human society view as a common currency, and to describe our behavior, energetics (economy), and dynamics based on ecological theory. Besides, the EST behaves promisingly as a gap analysis tool and a mean to address new research questions. Since its proposal, the EST has been further tested, namely in relation to the functioning and management of estuarine ecosystems and coastal areas, taking the Mondego Estuary (western Portugal) as core case study.",
url_article="https://www.sciencedirect.com/science/article/pii/S1470160X17306921",
title="Ecological Indicators",
doc="S1470160X17306921",
link_pdf="https://www.sciencedirect.com/science/article/pii/S1470160X17306921/pdfft?md5=eace66f2674b28acb3e743347dde0505&pid=1-s2.0-S1470160X17306921-main.pdf"
}
@article{MA201389,
title = "AN EVOLUTIONARY PERSPECTIVE ON TOURISM AREA DEVELOPMENT",
