@article{GROTE2020104709,
title = "Safety and autonomy: A contradiction forever?",
journal = "Safety Science",
volume = "127",
pages = "104709",
year = "2020",
issn = "0925-7535",
doi = "https://doi.org/10.1016/j.ssci.2020.104709",
url = "http://www.sciencedirect.com/science/article/pii/S0925753520301065",
author = "Gudela Grote",
keywords = "Process safety, Personal safety, Uncertainty, Safety rules, Safety leadership, Safety culture",
abstract = "Much of current research on designing safe organizations centers around the question of how to best manage concurrent demands for stability and flexibility in the face of external and internal uncertainties. Classic approaches to safety management have favored centralized decision-making in organizations which promises stability through hierarchical control and adherence to predefined procedures. Newer research has stressed the need for decentralized decision-making to enable flexibility through fast local adaptations and improvisation in unprecedented situations, thereby confronting the apparent contradiction between safety and autonomy. Especially research on high-reliability organizations (HROs) has aimed to resolve this contradiction by identifying mechanisms for bridging centralization and decentralization through what has been termed loose coupling. However, the contradiction still lives on in many debates in safety research, for instance concerning safety rules, safety leadership, and safety culture. Using these research domains as examples, it is argued in this paper that by distinguishing between process and personal safety and between operational and higher-order autonomy the relationship between safety and autonomy can be better understood and eventually better managed. Moreover, uncertainty is assumed to be a crucial moderating influence in this relationship. Based on this framework, propositions are developed to guide future research towards reconciling safety and autonomy in safety management.",
url_article="https://www.sciencedirect.com/science/article/pii/S0925753520301065",
title="Safety Science",
doc="S0925753520301065",
link_pdf="https://www.sciencedirect.com/science/article/pii/S0925753520301065/pdfft?md5=dc194c463552883a14360d89220e2dae&pid=1-s2.0-S0925753520301065-main.pdf"
}
@article{PENNY201837,
title = "Resilience principles in socio-hydrology: A case-study review",
journal = "Water Security",
volume = "4-5",
pages = "37 - 43",
year = "2018",
issn = "2468-3124",
doi = "https://doi.org/10.1016/j.wasec.2018.11.003",
url = "http://www.sciencedirect.com/science/article/pii/S2468312418300166",
author = "Gopal Penny and Jessica J. Goddard",
abstract = "Resilience is recognized as critical to socio-hydrology, but few studies systematically integrate theoretical developments from resilience research into model design and evaluation. This review presents a novel application of seven resilience principles to evaluate six coevolutionary models from socio-hydrology. The principles provide a structured framing of resilience grounded in decades of research characterizing social-ecological systems. We find that coevolutionary models incorporate aspects of all principles and that these influence socio-hydrologic system trajectories. Our synthesis demonstrates the potential for resilience principles to serve as a theoretical frame for model design, evaluation, and comparison. We emphasize underexplored dimensions of resilience in the models and encourage future research to experiment with these principles to broaden our understanding of resilience in coupled social-hydrologic systems.",
url_article="https://www.sciencedirect.com/science/article/pii/S2468312418300166",
title="Water Security",
doc="S2468312418300166",
link_pdf="https://www.sciencedirect.com/science/article/pii/S2468312418300166/pdfft?md5=eec1f3743a37bb4b4bb8a27a6eb8d452&pid=1-s2.0-S2468312418300166-main.pdf"
}
@article{HOSSEINI201647,
title = "A review of definitions and measures of system resilience",
journal = "Reliability Engineering & System Safety",
volume = "145",
pages = "47 - 61",
year = "2016",
issn = "0951-8320",
doi = "https://doi.org/10.1016/j.ress.2015.08.006",
