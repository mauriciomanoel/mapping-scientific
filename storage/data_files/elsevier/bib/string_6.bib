@article{NTUEN200778,
title = "LIVING SYSTEM THEORY AS A CYBERNETIC MODEL FOR DESIGN AND ANALYSIS OF ADAPTIVE HUMAN-COMPUTER INTERFACE",
journal = "IFAC Proceedings Volumes",
volume = "40",
number = "16",
pages = "78 - 83",
