}
@article{DENIZE2007968,
title = "Concerning trust and information",
journal = "Industrial Marketing Management",
volume = "36",
number = "7",
pages = "968 - 982",
year = "2007",
note = "Opening the network - Bridging the IMP tradition and other research perspectives",
issn = "0019-8501",
doi = "https://doi.org/10.1016/j.indmarman.2007.06.004",
url = "http://www.sciencedirect.com/science/article/pii/S0019850107000843",
author = "Sara Denize and Louise Young",
keywords = "Trust, Information exchange, Communication, Interfirm relationships, Relationship evolution",
abstract = "Communication and the associated information exchanges are key drivers of the development of relationships and of the trust embedded within them. This paper considers the development of business relationships in terms of the continuing co-evolution of trust and information exchange and the issues associated with researching these processes. The interconnections of trust and information exchange are examined in a survey of business relationships involving information exchange (n=355). Analysis of variance shows few, if any, of the aspects of the standard conceptualizations of information exchange are associated with increased levels of trust. It is information exchange norms that have the greatest (positive) association with the level of trust. These norms emerge as part of the long term co-production of the relationship itself. This has important managerial implications. We conclude there are few managerial actions involving managing communication that can “manufacture” trust and improve or develop relationships in the short term. The paper concludes with a discussion of alternative ways of envisaging communication and relationship management and the nature and future of research into the evolution of business relationships.",
url_article="https://www.sciencedirect.com/science/article/pii/S0019850107000843",
