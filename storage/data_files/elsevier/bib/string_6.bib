@article{JOLY20171,
title = "The future of computational biomedicine: Complex systems thinking",
journal = "Mathematics and Computers in Simulation",
volume = "132",
pages = "1 - 27",
year = "2017",
issn = "0378-4754",
doi = "https://doi.org/10.1016/j.matcom.2015.06.010",
