}
@article{SUOMALAINEN2011958,
title = "Software product roadmapping in a volatile business environment",
journal = "Journal of Systems and Software",
volume = "84",
number = "6",
pages = "958 - 975",
year = "2011",
issn = "0164-1212",
doi = "https://doi.org/10.1016/j.jss.2011.01.031",
