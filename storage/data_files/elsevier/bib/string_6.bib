@article{DEVEZAS2002317,
title = "The nonlinear dynamics of technoeconomic systems: An informational interpretation",
journal = "Technological Forecasting and Social Change",
volume = "69",
number = "4",
pages = "317 - 357",
year = "2002",
issn = "0040-1625",
doi = "https://doi.org/10.1016/S0040-1625(01)00155-X",
url = "http://www.sciencedirect.com/science/article/pii/S004016250100155X",
author = "Tessaleno C. Devezas and James T. Corredine",
keywords = "Long waves, Kondratieff waves, Socioeconomic growth, Logistic growth, Chaos theory, Systems science, Information theory, Cybernetics",
abstract = "In this paper, a cybernetic framework is proposed that may help in understanding the specifics of the timely unfolding of recurrent social phenomena, as well as provide a basis for their application as useful forecasting tools for futures studies. The long-wave behavior in technoeconomic development was chosen to apply this theoretical framework. The time evolution of a technoeconomic system is described discretely as a logistically growing number of “interactors” adopting an emerging set of basic social and technological innovations. By using the logistic function as the probabilistic distribution of individuals exchanging and processing information in a finite niche of available information, it is possible to demonstrate that the rate of information entropy change exhibits a “wavy” aspect evidenced by a four-phased behavior denoting the unfolding of a complete long wave. The entire unfolding process, divided into two cycles, an innovation cycle and a consolidation cycle, is analyzed, and two very important threshold points are identified and discussed. The present theoretical analysis suggests that the technoeconomic system is not a purely chaotic process, but exhibits a limit-cycle behavior, whose basic mechanism is the periodical deployment and filling of information in a “leeway” field of active information. The pace of the process, and hence the duration of the long wave, is determined by two biological control parameters, one cognitive, driving the rate of exchanging and processing information at the microlevel, and the other generational, constraining the rate of transfer of knowledge (information integrated into a context) between successive generations at the macrolevel. Moreover, it is speculated that social systems mimic living systems as efficient negentropic machines, and making use of Prigogine's entropy balance equation for open systems, it is suggested that its cyclical behavior is probably the best way to follow nature's efficiency strategy.",
url_article="https://www.sciencedirect.com/science/article/pii/S004016250100155X",
title="Technological Forecasting and Social Change",
doc="S004016250100155X",
link_pdf="https://www.sciencedirect.com/science/article/pii/S004016250100155X/pdfft?md5=1e8971d20d6c808448d45ae48b1847d1&pid=1-s2.0-S004016250100155X-main.pdf"
}
@article{MARKIN1975557,
title = "Information, decision-making, and the diagnostic process",
journal = "Comprehensive Psychiatry",
volume = "16",
number = "6",
pages = "557 - 572",
year = "1975",
issn = "0010-440X",
doi = "https://doi.org/10.1016/S0010-440X(75)80019-8",
url = "http://www.sciencedirect.com/science/article/pii/S0010440X75800198",
author = "Richard A. Markin",
abstract = "Summary
Two types of diagnostic signs, taken from an intake form used on applicants at a mental health center, were used to arrive at a diagnosis. This process was replicated with a computer program. This statistical model attempted to reflect the diagnostic process as it exists in the mind of the clinician. This paper has attempted to demonstrate that, while according to logic the two groups of diagnostic cues presented and worked with represent different universes of discourse, such divergent types of information do coexist in the mind of the clinician, and in spite of the variation in “meaning” that these cues represent (from the standpoint of differences within and between each class, in terms of logical type and level of complexity), they are used to achieve closure within the “inner space” of the interlocutor as he assesses the patient. The data suggest that these two types of information form units in the same sign system and also that their definition and application derive from their interrelationship; all such elements comprising a uniquely structured whole. The research demonstrates that once these elements are identified and combined into classes (factors), based on their structural relationship to a statistically derived axis, they can be organized according to a stochastic model that reflects the sequential nature of the diagnostic process. Thus this program can be used to generate a diagnostic label or category by recombining the unit classes according to principles derived from a semiexplicit conceptual definition of mental health and illness and a largely implicit set of assessment procedures. The diagnostic enterprise, as it exists in this study, was criticized from a number of perspectives. Most importantly, it was suggested that the diagnostic process is a complex perceptual, conceptual, and emotional act arising from a unique two-party transaction that exists in a larger context to which it is intimately related. Further, the position was advanced that the diagnostician makes use of many kinds of information not specified in the diagnostic sign repertoire (i.e., the lexicon taken as given was not exhaustive, and there are other unidentified modes of communication that coexist with the “given” set of diagnostic cues), which are combined according to rules of organization and transformation not contained in the computer program. Thus the diagnostic “act” is seen not as a linear stochastic process but as an ongoing series of interrelationships characteristic of complex open or semiopen systems that are goal-seeking and make use of positive and negative feedback loops that process many channels simultaneously and “correct” or adapt to the recognized discrepancies between internally and externally perceived patterns until closure (or a degree of certainty as yet not totally explicated) is achieved and a diagnosis assigned.",
url_article="https://www.sciencedirect.com/science/article/pii/S0010440X75800198",
title="Comprehensive Psychiatry",
doc="S0010440X75800198",
link_pdf="https://www.sciencedirect.com/science/article/pii/S0010440X75800198/pdfft?md5=db49a5b453479f487dbaf936ffb3a6ec&pid=1-s2.0-S0010440X75800198-main.pdf"
}
@incollection{PASK1963177,
title = "A Discussion of the Cybernetics of Learning Behaviour",
editor = "N. Wiener and J.P. Schadé",
series = "Progress in Brain Research",
publisher = "Elsevier",
volume = "2",
pages = "177 - 214",
year = "1963",
issn = "0079-6123",
doi = "https://doi.org/10.1016/S0079-6123(08)62069-5",
url = "http://www.sciencedirect.com/science/article/pii/S0079612308620695",
author = "Gordon Pask",
abstract = "Publisher Summary
Cyberneticians are naturally concerned with the logic of large systems and the logical calibre of the learning process. Physiological mechanisms are shown to be special cases of completely general systems exhibiting principles such as homeostasis and dynamic stability. The material dependence of a brain or an artifact suggests that manifestly coherent experience stems from a cohesive physical or intellectual environment of which the brain is a specialised part. A cybernetic model in which learning and cognition appear as components of an evolutionary process in a self organizing system is considered here. The model illuminates the role of “distributed” or “non-localized” functions and it possesses an interesting asymmetry which seems to account for some of the peculiar discontinuities in behavior associated with “attention” and “insight.” Recent work from other countries indicates a number of physiological mechanisms acting at a macromolecular level that may embody the required interactions.",
