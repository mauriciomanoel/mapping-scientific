@article{SZAFLARSKI2017280,
title = "Social factors in marijuana use for medical and recreational purposes",
journal = "Epilepsy & Behavior",
volume = "70",
pages = "280 - 287",
year = "2017",
note = "Cannabinoids and Epilepsy",
issn = "1525-5050",
