@article{GALAPPATHTHI2017156,
title = "Can start-up motives influence social-ecological resilience in community-based entrepreneurship setting? Case of coastal shrimp farmers in Sri Lanka",
journal = "Marine Policy",
volume = "86",
pages = "156 - 163",
year = "2017",
issn = "0308-597X",
doi = "https://doi.org/10.1016/j.marpol.2017.09.024",
url = "http://www.sciencedirect.com/science/article/pii/S0308597X17303226",
author = "Iroshani M. Galappaththi and Eranga K. Galappaththi and Sarath S. Kodithuwakku",
keywords = "Resilience, Start-up motives, Entrepreneurship, Coastal shrimp aquaculture, Sri Lanka",
abstract = "The aim of this paper is to understand the extent to which the start-up motives can influence social-ecological resilience in community-based entrepreneurship setting. The case study approach was used to study five coastal communities in northwestern Sri Lanka. Shrimp farmers’ start-up motives include both the desire to capitalize on opportunities to utilize their resources and skills (opportunity-driven) and the need to establish satisfactory sources of income (necessity-driven). They have come up with diverse entrepreneurial responses to face various stressors and shocks by using their existing capital types and their willingness for collaboration. Farmers’ coping capacity is demonstrated through their individual responses whereas adaptive and transformative capacities are demonstrated mostly through their community cooperative work. Findings also reveal that the start-up motives of small-scale shrimp farmers are less influential on their social-ecological resilience in the community setting. Instead, the key to social-ecological resilience lies with the understanding of the nature of stressors and their commitment for collective action.",
url_article="https://www.sciencedirect.com/science/article/pii/S0308597X17303226",
title="Marine Policy",
doc="S0308597X17303226",
link_pdf="https://www.sciencedirect.com/science/article/pii/S0308597X17303226/pdfft?md5=6c963d6fc6da81b315936e7a650512bc&pid=1-s2.0-S0308597X17303226-main.pdf"
}
@article{EDWARDS201426,
title = "Design of systems for productivity and well being",
journal = "Applied Ergonomics",
volume = "45",
number = "1",
pages = "26 - 32",
year = "2014",
note = "Systems Ergonomics/Human Factors",
issn = "0003-6870",
doi = "https://doi.org/10.1016/j.apergo.2013.03.022",
url = "http://www.sciencedirect.com/science/article/pii/S000368701300063X",
author = "Kasper Edwards and Per Langaa Jensen",
keywords = "Production system design, Complexity management, Stakeholder perspective",
abstract = "It has always been an ambition within the ergonomic profession to ensure that design or redesign of production systems consider both productivity and employee well being, but there are many approaches to how to achieve this. This paper identifies the basic issues to be addressed in light of some research activities at DTU, especially by persons responsible for facilitating design processes. Four main issues must be addressed: (1) determining the limits and scope of the system to be designed; (2) identifying stakeholders related to the system and their role in the system design; (3) handling the process' different types of knowledge; and (4) emphasizing that performance management systems, key performance indicators (KPIs), and leadership are also part of the system design and must be given attention. With the examples presented, we argue that knowledge does exist to help system design facilitators address these basic issues.",
url_article="https://www.sciencedirect.com/science/article/pii/S000368701300063X",
title="Applied Ergonomics",
doc="S000368701300063X",
link_pdf="https://www.sciencedirect.com/science/article/pii/S000368701300063X/pdfft?md5=0197ff2794c191a7c029fdbc88c468cd&pid=1-s2.0-S000368701300063X-main.pdf"
}
