title="Journal of Computational Science",
doc="S1877750316301168",
link_pdf="https://www.sciencedirect.com/science/article/pii/S1877750316301168/pdfft?md5=783600e211090aa47b2a9ca7fc4ece34&pid=1-s2.0-S1877750316301168-main.pdf"
}
@article{BRADSHAW2001460,
title = "Ecology and social responsibility: the re-embodiment of science",
