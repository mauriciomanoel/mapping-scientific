}
@article{SMIRNOV2015290,
title = "Service-Based Socio-Cyberphysical Network Modeling for Guided Self-Organization",
journal = "Procedia Computer Science",
volume = "64",
pages = "290 - 297",
year = "2015",
note = "Conference on ENTERprise Information Systems/International Conference on Project MANagement/Conference on Health and Social Care Information Systems and Technologies, CENTERIS/ProjMAN / HCist 2015 October 7-9, 2015",
issn = "1877-0509",
doi = "https://doi.org/10.1016/j.procs.2015.08.492",
url = "http://www.sciencedirect.com/science/article/pii/S1877050915026277",
author = "Alexander Smirnov and Nikolay Shilov",
keywords = "socio-cyberphysical system, socio-inspired service, service-based modeling, behavior modeling, behavior pattern",
abstract = "Socio-cyberphysical networks tightly integrate humans, real-world objects and IT infrastructure. In some cases of cyberphysical networks, their configuration can be done in a centralized way (e.g., automated production line configuration). However, for socio-cyberphysical networks the centralized control is not possible due to the independence and autonomy of the network members (e.g., employees of an enterprise). The paper proposes an approach to model such network via a set of interacting socio-inspired services. Such services are capable to model behavior of the network participants taking into account their preferences, strategies and social norms. The interoperability of the services is achieved due to the usage of common standards (such as WSDL and SOAP) at the technological level, and common ontology at the semantic level. The developed conceptual model of socio-inspired service is presented together with behavior patterns and methods for their processing.",
url_article="https://www.sciencedirect.com/science/article/pii/S1877050915026277",
title="Procedia Computer Science",
doc="S1877050915026277",
link_pdf="https://www.sciencedirect.com/science/article/pii/S1877050915026277/pdfft?md5=52b4a95a1fbe5ab75546b354051977d9&pid=1-s2.0-S1877050915026277-main.pdf"
}
@article{SANCHEZGUINEA2016251,
title = "A systematic review on the engineering of software for ubiquitous systems",
journal = "Journal of Systems and Software",
volume = "118",
pages = "251 - 276",
year = "2016",
issn = "0164-1212",
doi = "https://doi.org/10.1016/j.jss.2016.05.024",
url = "http://www.sciencedirect.com/science/article/pii/S0164121216300553",
author = "Alejandro {Sánchez Guinea} and Grégory Nain and Yves {Le Traon}",
keywords = "Empirical software engineering, Evidence-based software engineering, Systematic review, Research synthesis, Software development cycle, Ubiquitous systems, Development methods, Pervasive systems",
abstract = "Context: Software engineering for ubiquitous systems has experienced an important and rapid growth, however the vast research corpus makes it difficult to obtain valuable information from it. Objective: To identify, evaluate, and synthesize research about the most relevant approaches addressing the different phases of the software development life cycle for ubiquitous systems. Method: We conducted a systematic literature review of papers presenting and evaluating approaches for the different phases of the software development life cycle for ubiquitous systems. Approaches were classified according to the phase of the development cycle they addressed, identifying their main concerns and limitations. Results: We identified 128 papers reporting 132 approaches addressing issues related to different phases of the software development cycle for ubiquitous systems. Most approaches have been aimed at addressing the implementation, evolution/maintenance, and feedback phases, while others phases such as testing need more attention from researchers. Conclusion: We recommend to follow existing guidelines when conducting case studies to make the studies more reproducible and closer to real life cases. While some phases of the development cycle have been extensively explored, there is still room for research in other phases, toward a more agile and integrated cycle, from requirements to testing and feedback.",
url_article="https://www.sciencedirect.com/science/article/pii/S0164121216300553",
title="Journal of Systems and Software",
doc="S0164121216300553",
link_pdf="https://www.sciencedirect.com/science/article/pii/S0164121216300553/pdfft?md5=307a96a7264ef128e2f78ecaf6c87d45&pid=1-s2.0-S0164121216300553-main.pdf"
}
@article{MORALES2019235,
