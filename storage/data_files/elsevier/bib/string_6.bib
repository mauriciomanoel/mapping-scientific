doc="S0167923698000657",
link_pdf="https://www.sciencedirect.com/science/article/pii/S0167923698000657/pdfft?md5=7a8d69e3867dcb13793b4055b5ce2e3b&pid=1-s2.0-S0167923698000657-main.pdf"
}
