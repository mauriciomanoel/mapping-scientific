pages = "79--90",
volume = "43",
publisher = "Elsevier B.V.",
year = "2016",
title = "Service composition for IP smart object using realtime Web protocols: Concept and research challenges",
language = "eng",
author = "Han, Son N. and Khan, Imran and Lee, Gyu Myoung and Crespi, Noel and Glitho, Roch H.",
keywords = "Internet of Things ; Smart Object ; Service Composition",
month = "January",
