year = "2016",
title = "Medical Internet of Things and Big Data in Healthcare",
author = "Dimitrov, Dimiter V.",
keywords = "Review Article ; Telemedicine ; Smartphone ; Mobile Applications ; Wireless Technology ; Disease Management",
month = "July",
}

