language = "eng ; slv",
address = "Kranj",
author = "Žuraj, Milica and Šparl, Petra and Žnidaršič, Anja",
keywords = "Online Shopping ; Markets ; Consumers ; Factor Analysis ; Shopping ; Purchasing ; Studies ; Browsing ; Consumers ; Clothing ; Technology Utilization ; Scanning ; Three Dimensional Bodies ; Online Shopping Apparel ; Custom Made Apparel ; Consumers’ Shopping Experience",
