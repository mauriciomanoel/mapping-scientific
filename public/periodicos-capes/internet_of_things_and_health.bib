language = "eng",
author = "Fisher, Roy and Hancke, Gerhard",
keywords = "Transport Layer Security (Network Protocol) – Methods ; Data Security – Methods ; Internet of Things – Methods ; Streaming Media – Methods",
month = "August",
}

