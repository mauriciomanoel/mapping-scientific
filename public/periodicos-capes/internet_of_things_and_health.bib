volume = "27",
publisher = "Information Today, Inc.",
number = "3",
year = "2013",
title = "The M2M EXPLOSION: How to Achieve Faster, Smarter Data Analysis in a Connected World",
language = "eng",
address = "Chatham",
author = "Deloach, Don",
keywords = "United States–Us ; Data Analysis ; Information Management ; Information Technology ; Smart Grid Technology ; Measuring Instruments ; Communications & Information Management ; United States",
month = "September",
url = "http://search.proquest.com/docview/1443773936/?pq-origsite=primo",
