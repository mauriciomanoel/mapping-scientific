keywords = "Studies ; Internet of Things ; Cloud Computing ; Software Services ; Virtualization ; Information Technology Management ; Software & Computer Services Industry ; Experiment/Theoretical Treatment",
month = "September",
url = "http://search.proquest.com/docview/1810341134/",
}

@article{BaeckePhilippe2017Tvov,
issn = "01679236",
abstract = "The advent of the Internet of Things enables companies to collect an increasing amount of sensor generated data which creates plenty of new business opportunities. This study investigates how this sensor data can improve the risk selection process in an insurance company. More specifically, several risk assessment models based on three different data mining techniques are augmented with driving behaviour data collected from In-Vehicle Data Recorders. This study proves that including standard telematics variables significantly improves the risk assessment of customers. As a result, insurers will be better able to tailor their products to the customers' risk profile. Moreover, this research illustrates the importance of including industry knowledge, combined with data expertise, in the variable creation process. Especially when a regulator forces the use of easily interpretable data mining techniques, expert-based telematics variables are able to improve the risk assessment model in addition to the standard telematics variables. Further, the results suggest that if a manager wants to implement Usage-Based-Insurances, Pay-As-You-Drive related variables are most valuable to tailor the premium to the risk. Finally, the study illustrates that this new type of telematics-based insurance product can quickly be implemented since three months of data is already sufficient to obtain the best risk estimations.",
journal = "Decision Support Systems",
volume = "98",
publisher = "Elsevier Sequoia S.A.",
year = "2017",
title = "The value of vehicle telematics data in insurance risk selection processes",
language = "eng",
address = "Amsterdam",
author = "Baecke, Philippe and Bocca, Lorenzo",
keywords = "Studies ; Internet of Things ; Telematics ; Data Mining ; Vehicles ; Automobile Insurance ; Customers ; Data Mining ; Risk Assessment ; Insurance ; Data Recorders ; Mathematical Models ; Recorders ; Telematics ; Deduction (Ci)",
month = "June",
url = "http://search.proquest.com/docview/1927143533/?pq-origsite=primo",
}

@article{PradillaJuan2015SLSO,
issn = "1548-0992",
abstract = "Internet of Things (IoT) platforms interoperability is a major issue in current IoT scenarios. There are different mechanisms to achieve interoperability and Open Geospatial Consortium (OGC) with Sensor Web Enablement (SWE) block of standards like Sensor Observation Service (SOS) has provided the ability to interoperate at different layers, following a centralized approach. However, IoT nature is distributed and a new approach is needed. At the same time, current SOS implementations provide several services and components that are rarely used and affect performance, and are mainly focused for large sensor deployments. In this paper we propose a lightweight SOS design, named SOSLite, based in the OGC consolidated SWE standards able to be executed in low resources devices (e.g. RaspberryPi) and allow fast interoperability and scalability. The proposed use case in which the proposed system has been tested is m-health and concretely ambient assisted living.",
