title = "CIOs must stand at the forefront of innovation",
language = "eng",
address = "Framingham",
author = "Stofko, Larry",
keywords = "Innovations ; Internet of Things ; Information Technology",
month = "October",
url = "http://search.proquest.com/docview/1830472531/",
}

